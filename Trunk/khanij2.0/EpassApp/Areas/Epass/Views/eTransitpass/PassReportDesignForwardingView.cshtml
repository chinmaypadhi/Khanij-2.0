                                                    @(Html.DisplayFor(model => model.TareWeight_Qty)) <label> @Model.UnitName</label>
                                                }
                                                else
                                                {
                                                    <br />
                                                    <span></span>
                                                }
                                            }
                                            else
                                            {
                                                @(Html.DisplayFor(model => model.TareWeight_Qty)) <label> @Model.UnitName</label>
                                            }

                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
