
@{
    ViewData["Title"] = "StatusView";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}
<script>
    excelMe = "yes"
    pdfMe = "yes"
    printMe = "yes"
    $(document).ready(function () {
        loadNavigation('StatusView', 'glhelpdesk', 'plraiseticket', 'tl', 'Configuration', 'View Status Update', ' ');
        $('#datatable').DataTable();
        $('.shorting-lnk').click(function () {
            $('.shorting-lnk i').toggleClass('fa-sort-amount-up-alt fa-sort-amount-down-alt ');
        });
    });
</script>
<section class="wrapper main-wrapper row">
    <div class="col-12">
        <!-- MAIN CONTENT AREA STARTS -->
        <div class="row">
            <div class="col-12">
                <div class="main-tab">
                    <ul class="nav nav-tabs">
                        <li class="nav-item">
                            <a class="nav-link" href="~/Configuration/Home/StatusUpdate">
                                Add
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" href="javascript:void(0);">
                                View
                            </a>
                        </li>
                    </ul>
                    @Html.Partial("_util")
                </div>
                <section class="box form-container">
                    <div class="content-body">
                        <h5 class="text-brown font-weight-bold mt-0">User Status Updation Details</h5>
                        <div class="content-body pt-0">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="table-responsive" id="viewtable">
                                        <table id="datatable" class="table table-sm table-bordered">
                                            <thead>
                                                <tr>
                                                    <th width="30px">Sl#</th>
                                                    <th>District Name</th>
                                                    <th>User Type</th>
                                                    <th>User Name</th>
                                                    <th>Status</th>
                                                    <th>Action Date</th>
                                                    <th width="100px" class="noprint">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr class="active-row">
                                                    <td>1</td>
                                                    <td>Balod</td>
                                                    <td>Lessee</td>
                                                    <td>Ambuja Cement Limited</td>
                                                    <td>Wroking</td>
                                                    <td>25-Feb-2021</td>
                                                    <td class="noprint">
                                                        <a href="~/Configuration/Home/StatusUpdate" class="btn-floating btn-outline-primary btn-sm waves-effect waves-light">Edit</a>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
    <!-- MAIN CONTENT AREA ENDS -->
</section>


