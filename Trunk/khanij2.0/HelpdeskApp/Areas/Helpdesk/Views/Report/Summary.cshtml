        $('.datetime').datetimepicker({
            format: 'LT',
            format: "DD/MM/YYYY",
            daysOfWeekDisabled: [],
            minDate: firstDay,
            widgetPositioning: {
                horizontal: 'auto',
                vertical: 'bottom'
            }
        }).keydown(false);
        var today = new Date();
        var mm = today.getMonth() + 1; //January is 0!
        var yyyy = today.getFullYear();
        if (mm < 10) {
            mm = '0' + mm;
        }
        var firstDay = '01' + '/' + mm + '/' + yyyy;
        $("#fromdate").val(firstDay);
        $("#IssueStatus").val('Pending');
        getIssueSummaryReports();
    });
