                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/KHASARA_PANCHASALA/@Model.KHASARA_PANCHSALA_FILE_NAME' title="" class="ml-2" data-original-title="Download" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                    }
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Map / Plan of applied area</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <span class="colon">:</span>@Model.MAP_PLAN_OF_APPLIED_AREA_FILE_NAME
                                    @if (Model.MAP_PLAN_OF_APPLIED_AREA_FILE_NAME != null && Model.MAP_PLAN_OF_APPLIED_AREA_FILE_NAME != "" && Model.MAP_PLAN_OF_APPLIED_AREA_FILE_NAME != "--")
                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/MAP_PLAN_OF_APPLIED_AREA/@Model.MAP_PLAN_OF_APPLIED_AREA_FILE_NAME' title="" class="ml-2" data-original-title="Download" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                    }
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Forest Report</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <span class="colon">:</span>@Model.FOREST_REPORT_FILE_NAME
                                    @if (Model.FOREST_REPORT_FILE_NAME != null && Model.FOREST_REPORT_FILE_NAME != "" && Model.FOREST_REPORT_FILE_NAME != "--")
                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/FOREST_REPORT/@Model.FOREST_REPORT_FILE_NAME' title="" class="ml-2" data-original-title="Download" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                    }
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Revenue Officer Report</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <span class="colon">:</span>@Model.REVENUE_OFFICER_REPORT_FILE_NAME
                                    @if (Model.REVENUE_OFFICER_REPORT_FILE_NAME != null && Model.REVENUE_OFFICER_REPORT_FILE_NAME != "" && Model.REVENUE_OFFICER_REPORT_FILE_NAME != "--")
                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/REVENUE_OFFICER_REPORT/@Model.REVENUE_OFFICER_REPORT_FILE_NAME' title="" class="ml-2" data-original-title="Download" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                    }
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Spot Inspection and Analysis Report</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <span class="colon">:</span>@Model.SPOT_INSPECTION_AND_ANALYSIS_REPORT_FILE_NAME
                                    @if (Model.SPOT_INSPECTION_AND_ANALYSIS_REPORT_FILE_NAME != null && Model.SPOT_INSPECTION_AND_ANALYSIS_REPORT_FILE_NAME != "" && Model.SPOT_INSPECTION_AND_ANALYSIS_REPORT_FILE_NAME != "--")
                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/SPOT_INSPECTION_AND_ANALYSIS_REPORT/@Model.SPOT_INSPECTION_AND_ANALYSIS_REPORT_FILE_NAME' title="" class="ml-2" data-original-title="Download" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
