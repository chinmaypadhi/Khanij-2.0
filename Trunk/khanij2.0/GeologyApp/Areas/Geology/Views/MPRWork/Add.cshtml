    var totalRowsCount = 1;
    function addRow() {
        debugger;
        var date = /^(0?[1-9]|1[0-9]|2[0-9]|3[0-1])\/(0?[1-9]|1[0-2])\/([0-9]{4})$/;
        if (!blankFieldValidation("Camps_Name", "Camp Name")) {
            return false;
        }
        else if (!blankFieldValidation("Camps_Designation", "Camp Designation")) {
            return false;
        }
        else if (!blankFieldValidation("Camps_From", "Camps From Date")) {
            return false;
        }
        else if (!date.test($('#Camps_From').val())) {
            swal("", "Enter Valid Camps From Date", "warning");
            $('#Camps_From').focus();
            return false;
        }
        else if (!blankFieldValidation("Camps_To", "Camps To Date")) {
            return false;
        }
        else if (!date.test($('#Camps_To').val())) {
            swal("", "Enter Valid Camps To Date", "warning");
            $('#Camps_To').focus();
            return false;
        }
        else if (!blankFieldValidation("Camps_No_Of_Days", "No Of days")) {
            return false;
