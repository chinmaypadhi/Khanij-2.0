@*// *********************************************************************************************************
    //  View Name                : ViewLesseeList
    //  Desciption               : To Lessee Users by User,Role type in view
    //  Created By               : Lingaraj Dalai
    //  Created On               : 26 April 2022
    // ********************************************************************************************************
*@
@model IEnumerable<MasterEF.LesseeListForDGMModel>
@using MasterApp.Web;
@using MasterEF;
@using Microsoft.Extensions.Configuration
@using Microsoft.AspNetCore.Http;
@using Microsoft.Extensions.Options;
@inject IConfiguration configuration
@inject IHttpContextAccessor HttpContextAccessor
@inject IOptions<KeyList> _objKeyList

@{
    ViewData["Title"] = "ViewLesseeList";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}
<script>
    backMe = "yes"
    $(document).ready(function () {
        loadNavigation('ViewLesseeList', 'glRoleRes', 'plUser', 'tl', 'View Lessee Profile', 'Lessee Information', ' ');
    });
</script>

<section class="wrapper main-wrapper row">
    <div class="col-12">
        @Html.Partial("_CMSnavigation")
        <!-- MAIN CONTENT AREA STARTS -->
        <div class="row">
            <div class="col-12">
                <div class="main-tab">
                    <ul class="nav nav-tabs">
                        <li class="nav-item">
                            <a class="nav-link active">
                                Lessee Information
                            </a>
                        </li>
                    </ul>
                    @Html.Partial("_CMSutil")
                    @Html.AntiForgeryToken()
                </div>
                <section class="box form-container">
                    <div class="content-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <table id="table-exports" class="table table-sm table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Sl#</th>
                                            <th>District Name</th>
                                            <th>Lessee Name</th>
                                            <th>Lessee Code</th>
                                            <th>Lessee Type</th>
                                            <th>Lessee Status</th>
                                            <th>Mines Name</th>
                                            <th>Address</th>
                                            <th>Email</th>
                                            <th>Mobile No</th>
                                            <th>Total Lease Area (Hect)</th>
                                            <th>Lease Period (In years)</th>
                                            <th>Mineral</th>
                                            <th>Mineral Form</th>
                                            <th>Mineral Grade</th>
                                            <th>Internet Connection</th>
                                            <th>Status of Lessee</th>
                                            <th>Request Submission Date</th>
                                            <th>Request Approval Date</th>
                                            <th class="text-center noprint">View Details</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{
                                                var count = 0;
                                                foreach (var item in Model)
                                                {
                                                    count++;
                                            <tr>
                                                <td>@count</td>
                                                <td>@item.DistrictName</td>
                                                <td>@item.ApplicantName</td>
                                                <td>@item.LeaseCode</td>
                                                <td>@item.lesseeType</td>
                                                <td>@item.StatusOfLessee</td>
                                                <td>@item.MineName</td>
                                                <td>@item.Address</td>
                                                <td>@item.EMailId</td>
                                                <td>@item.MOBILENO</td>
                                                <td>@item.TotalLeaseArea</td>
                                                <td>@item.LeasePeriod</td>
                                                <td>@item.MineralDealswith</td>
                                                <td>@item.FormDealswith</td>
                                                <td>@item.GradeDealswith</td>
                                                <td>@item.IsInterNetConnectionAtDistpatch</td>
                                                <td>@item.StatusOfLesseeLicensee</td>
                                                <td>@item.RequestSubmissionDate</td>
                                                <td>@item.RequestApprovalDate</td>
                                                <td class="text-center noprint">
                                                    <a href="@CustomQueryStringHelper.EncryptString("LesseeProfile", "Dashboard", "Profile", new { UserId = item.LesseId,MineralType=item.MineralType,LeaseCode=item.LeaseCode,ApplicantName=item.ApplicantName })"
                                                       class="btn-floating btn-outline-primary btn-sm" title="View Details">
                                                        <i class="icon-street-view-solid"></i>
                                                    </a>
                                                </td>
                                            </tr>
                                                }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>

