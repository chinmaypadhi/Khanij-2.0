                            "<td>" + item.capacity + "</td>" +
                            "<td>" + item.capacity_Unit + "</td>" +
                            "<td>" + item.noofMachinery + "</td>" +
                            "<td>" + item.type_Electrical + "</td>" +
                            "<td>" + item.used_Area + "</td>" +
                            "<td class='noprint text-center'>" +
                            "<a onclick='EditMachineryDetails(this.parentNode.parentNode.rowIndex-1,this.id);' id='" + item.machineryType_Id + "' data-toggle='modal' data-target='.alert-modal' class='btn-floating btn-outline-primary btn-sm' data-toggle='tooltip' data-placement='top' title='Edit'><i class='icon-pencil-alt-solid'></i></a>" +
