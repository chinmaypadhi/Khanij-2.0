                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row mt-3">
                                        <div class="col-sm-12 text-center">
                                            <a class="btn btn-dark btn-md ml-0 waves-effect waves-light btnPrevious" onclick="showThird()">Previous</a>
                                            <a href="@Url.Content("~/Geology/MPRWork/Add")" class="btn btn-danger btn-md ml-0 waves-effect waves-light">Reset</a>
                                            <input type="submit" value='@ViewBag.Button' class="btn btn-primary btn-md ml-0 waves-effect waves-light" name="Submit" id="Submit" onclick="return Validate()" />
                                        </div>
                                    </div>

                                </div>
                            </div>
                            @Html.HiddenFor(m => m.MPR_Id, new { @id = "hfMPRId" });
                            @Html.HiddenFor(m => m.Camps_Namedetails, new { @id = "hfCampsName" })
                            @Html.HiddenFor(m => m.Camps_Designationdetails, new { @id = "hfcampsDesg" })
                            @Html.HiddenFor(m => m.Camps_Fromdetails, new { @id = "hfcampsFrom" })
                            @Html.HiddenFor(m => m.Camps_Todetails, new { @id = "hfcampsTo" })
                            @Html.HiddenFor(m => m.Camps_No_Of_Daysdetails, new { @id = "hfcampsNodays" })
                            @Html.HiddenFor(m => m.Camps_Remarksdetails, new { @id = "hfcampsRemarks" })
                            @Html.HiddenFor(m => m.Work_Id, new { @id = "hfWorkId" })
                        }
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>


