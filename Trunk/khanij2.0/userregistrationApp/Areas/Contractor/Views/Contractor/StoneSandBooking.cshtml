
@{
    ViewData["Title"] = "StoneSandBooking";
    Layout = "~/Views/Shared/_WebHeaderLayout.cshtml";
}
@inject Microsoft.Extensions.Options.IOptions<userregistrationEF.KeyList> metaOptions

@{
    var options = metaOptions.Value;
}
<style>
    .refesh-btn {
        position: absolute;
        right: 59%;
        top: 2px;
        font-size: 25px;
        border: 1px solid #ddd;
        padding: 1px 10px;
        background-color: #dddddd6e;
        border-radius: 5px;
    }
</style>
<div class="inner-naviagtion">
    <div class="container">
        <nav aria-label="breadcrumb mb-0">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@options.HomeUrl"><span class="fas fa-home"></span></a></li>
                <li class="breadcrumb-item text-white" aria-current="page">Registrations</li>
                <li class="breadcrumb-item text-white" aria-current="page">Sand & Stone</li>
            </ol>
        </nav>
    </div>
</div>
<div class="inner-content">
    <div class="container">
        <h3 class="heading">Sand & Stone Booking Registration</h3>


        <div class="row">
            <div class="mt-4 col-lg-12"><h4 class="text-primary">Purchaser Details</h4><hr /></div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Name <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">State <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <select class="form-control  searchableselect">
                                <option>Select</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">District <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <select class="form-control  searchableselect">
                                <option>Select</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Block <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <select class="form-control  searchableselect">
                                <option>Select</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Delivery Address <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <textarea class="form-control" rows="2"></textarea>
                            <small class="text-danger">Maximum <strong>500</strong> characters</small>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Pin Code<span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">
                        Mobile No. <span class="text-danger">*</span>
                        <a href="#" class="extra-information" data-toggle="tooltip" data-placement="top" title="OTP will be sent on your mobile number">
                            <i class="fas fa-info-circle"></i>
                        </a>
                    </label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Email Id</label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>
            </div> 
            <div class="mt-4 col-lg-12"><h4 class="text-primary">Mineral and Mines / Licensee</h4><hr /></div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Mineral Name <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <select class="form-control  searchableselect">
                                <option>Select</option>
                                <option>Sand</option>
                                <option>Stone</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">District Name <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <select class="form-control  searchableselect">
                                <option>Select</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">User Type <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <select class="form-control  searchableselect">
                                <option>Select</option>
                                <option>Lessee</option>
                                <option>Licensee</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Lessee / Licensee Name and KO Id <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <select class="form-control  searchableselect">
                                <option>Select</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Mineral Form <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <select class="form-control  searchableselect">
                                <option>Select</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Mineral Quantity in CFT<span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Purpose<span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <textarea class="form-control" rows="2"></textarea>
                            <small class="text-danger">Maximum <strong>500</strong> characters</small>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6"></div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Enter OTP <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6"></div>
            <div class="col-lg-6">
                <div class="row">
                    <label class="col-lg-5 col-md-5 col-12 col-form-label ">Enter Captcha <span class="text-danger">*</span></label>
                    <div class="col-lg-7 col-md-7 col-12">
                        <div class="form-group">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="row">
                    <img src="/CMSimg/captcha.png" alt="captcha image">
                    <a href="#" class="refesh-btn"><span class="fas fa-sync"></span></a>
                </div>
            </div>

        </div>

        <div class="row mt-3">
            <div class="col-lg-12">
                <button type="button" class="btn btn-success btn-lg mr-2" data-toggle="modal" data-target="#security-msg">Submit</button>
                <button type="button" class="btn btn-danger btn-lg">Reset</button>
            </div>
        </div>







    </div>
</div>


<div class="modal fade" id="security-msg" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-body text-center">
                <i class="fas fa-exclamation-triangle" style="color: #ffc1077a;font-size: 5rem;"></i>
                <h5 class=" mt-4">
                    Please enter the OTP <br /> which has sent on your Mobile Number
                </h5>
            </div>
            <div class="modal-footer justify-content-center">
                <button type="button" class="btn btn-danger" data-dismiss="modal" data-toggle="modal" data-target="#succcess-msg">Ok</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="succcess-msg" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-body text-center">
                <i class="fas fa-check-circle" style="color: #76b333;font-size:5rem;"></i>
                <h4 class=" mt-4">Successfully Registered </h4>
                <h5 class=" mt-4"> <strong>User id</strong>  and <strong>Password</strong> has send to your provided <strong>Email Id</strong> and <strong>Mobile Number</strong>.</h5>
            </div>
            <div class="modal-footer justify-content-center">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Ok</button>
            </div>
        </div>
    </div>
</div>

