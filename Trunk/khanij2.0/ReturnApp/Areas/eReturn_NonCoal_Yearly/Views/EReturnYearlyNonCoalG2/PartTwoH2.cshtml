                                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                                        <input type="number" value="@Model.objStaffWork.Whollyemployed4" name="objStaffWork.Whollyemployed4" id="objStaffWork_Whollyemployed4" onchange="TextChangeWhollyemployed()" class="form-control mb-lg-0 mb-3 StaffDetails" placeholder="Wholly employed">
                                                    </div>
                                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                                        <input type="number" value="@Model.objStaffWork.Partlyemployed4" name="objStaffWork.Partlyemployed4" id="objStaffWork_Partlyemployed4" onchange="TextChangePartlyemployed()" class="form-control StaffDetails" placeholder="Partly employed">
                                                    </div>


                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xl-6">
