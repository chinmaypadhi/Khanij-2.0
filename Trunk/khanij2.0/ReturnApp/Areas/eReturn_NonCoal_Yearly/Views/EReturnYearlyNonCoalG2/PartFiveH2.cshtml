    function TypeOfMachinery() {
        $.ajax({
            url: "/eReturn_NonCoal_Yearly/EReturnYearlyNonCoalG2/TypeOfMachinery",
            data: {},
            dataType: 'json',
            type: 'post',
            success: function (data) {
                $('#TypeofMachinery').html('');
                $('#TypeofMachinery').append('<option value="">Select</option>');
                if (data.length > 0) {
                    $.each(data, function (ind, item) {
                        if ($('#hdnTypeofMachinery').val() == item.typeofMachinery) {
                            $('#TypeofMachinery').append("<option selected='selected' value='" + item.typeofMachinery + "'>" + item.typeofMachinery + "</option>");
                        }
                        else {
                            $('#TypeofMachinery').append("<option value='" + item.typeofMachinery + "'>" + item.typeofMachinery + "</option>");
                        }
                    });
                }
            },
            error: function (err) {
                alert(err.responseText);
            }
        });
    }

    function GetMachineryDetails() {
        $.ajax({
            url: "/eReturn_NonCoal_Yearly/EReturnYearlyNonCoalG2/GetMachineryDetails",
            data: { 'Year': $('#Year').val() },
            dataType: 'json',
            type: 'post',
            success: function (data) {
