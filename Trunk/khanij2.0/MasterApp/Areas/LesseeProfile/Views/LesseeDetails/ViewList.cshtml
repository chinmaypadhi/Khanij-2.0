                                    @if (Model.TIN_CARD_SCAN_COPY_FILE_NAME != null)
                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span>@(Model.TIN_CARD) <a href='~/Upload/Lessee/APPLICATION/TIN_CARD/@Model.TIN_CARD_SCAN_COPY_FILE_NAME' title="" class="ml-2" data-original-title="Download GST Copy" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                    }
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Firm Registration Deed No.</label>
