                                    <h5 class="text-brown font-weight-bold">1 Details of the Mine </h5>
                                    <div class="form-group row">
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">Registration No.<small> (allotted by IBM)</small> </label>
                                            @Html.TextBoxFor(m => m.objMineDetails.Registrationnumber, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">Mine Code<small> (allotted by IBM)</small></label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MINE_CODE, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">Name of the Mineral</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MineralName, new { @class = "form-control" })
                                        </div>

                                    </div>

                                    <div class="form-group row">
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">Name of Mine</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MineName, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-4">
                                            <label for="inputLoginId" class="col-form-label font-weight-bolder">Name(s) of other mineral(s), if any,produced from the same mine</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.OtherMineral, new { @class = "form-control" })
                                        </div>


                                    </div>

                                    <h5 class="text-brown font-weight-bold">Location of the mine</h5>

                                    <div class="form-group row">
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Village</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MineVillage, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Post Office</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MinePostOffice, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label for="inputLoginId" class="col-form-label font-weight-bolder">Tahsil/Taluk</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MineTehsil, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">District</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MineDistrict, new { @class = "form-control" })
                                        </div>

                                    </div>

                                    <div class="form-group row">
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">State</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MineState, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">PIN Code</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MinePinCode, new { @maxlength = "6", @class = "form-control MinePinCode" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Fax No.</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MineFaxNo, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">E-mail Id</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MineEmailID, new { @class = "form-control" })
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Mobile No.</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MineMobileNo, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Phone No.</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.MinePhoneNo, new { @class = "form-control" })
                                        </div>
                                    </div>




                                    <h5 class="text-brown font-weight-bold">3 Name and address of Lessee/Owner (along with fax no and e-mail)</h5>
                                    <div class="form-group row">
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Name of Lessee/Owner</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.LesseeName, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Address</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.LesseeAddress, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label for="inputLoginId" class="col-form-label font-weight-bolder">District</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.LesseeDistrictName, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">State</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.LesseeStateName, new { @class = "form-control" })
                                        </div>

                                    </div>
                                    <div class="form-group row">
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">PIN Code</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.LesseePinCode, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Fax no</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.LesseeFaxNo, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label for="inputLoginId" class="col-form-label font-weight-bolder">E-mail</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.LesseeEmailID, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Mobile</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.LesseeMobileNo, new { @class = "form-control" })
                                        </div>

                                    </div>
                                    <div class="form-group row">
                                        <div class="col-sm-3">
                                            <label class="col-form-label font-weight-bolder">Phone No</label>
                                            @Html.TextBoxFor(m => m.objMineDetails.LesseePhoneNo, new { @class = "form-control" })
                                        </div>


                                    </div>

                                    <div class="row mt-3">
                                        <div class="col-sm-12 text-center">
                                            <a id="second-tab" data-toggle="tab" href="#LAD" class="btn btn-primary btn-md ml-0 waves-effect waves-light btnNext" role="tab" aria-controls="second" aria-selected="false">Next</a>
                                        </div>
                                    </div>



                                </div>
                                <div class="tab-pane fade" id="LAD" role="tabpanel" aria-labelledby="second-tab">
                                    @Html.HiddenFor(m => m.objOtherDetails.Flag1)
                                    @Html.HiddenFor(m => m.objOtherDetails.OtherDetails_Id)
                                    @Html.HiddenFor(m => m.objOtherDetails.MinePostOffice)
                                    @Html.HiddenFor(m => m.objOtherDetails.MineFaxNo)
                                    @Html.HiddenFor(m => m.objOtherDetails.LesseeFaxNo)

                                    <div class="form-group row">
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">4 Registered Office of the Lessee</label>
                                            @Html.TextBoxFor(m => m.objOtherDetails.RegisteredOffice, new { @class = "MineDetails form-control" })
                                        </div>
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">5 Director in charge</label>
                                            @Html.TextBoxFor(m => m.objOtherDetails.Director_in_charge, new { @class = "MineDetails form-control" })
                                        </div>
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">6 Agent</label>
                                            @Html.TextBoxFor(m => m.objOtherDetails.Agent, new { @class = "MineDetails form-control" })
                                        </div>



                                    </div>

                                    <div class="form-group row">
                                        <div class="col-sm-4">
                                            <label for="inputLoginId" class="col-form-label font-weight-bolder">7 Manager</label>
                                            @Html.TextBoxFor(m => m.objOtherDetails.Manager, new { @maxlength = "6", @class = "MineDetails form-control" })
                                        </div>
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">8 Mining Engineer in charge</label>
                                            @Html.TextBoxFor(m => m.objOtherDetails.MiningEngineer_in_charge, new { @class = "MineDetails form-control" })
                                            <small class="text-danger">Maximum <strong>500</strong> characters</small>
                                        </div>
                                        <div class="col-sm-4">
                                            <label for="inputLoginId" class="col-form-label font-weight-bolder">9 Geologist in charge</label>
                                            @Html.TextBoxFor(m => m.objOtherDetails.Geologist_in_charge, new { maxlength = "6", @class = "MineDetails form-control" })
                                        </div>




                                    </div>

                                    <div class="row">
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">10 Transferor (previous owner), if any</label>
                                            @Html.TextBoxFor(m => m.objOtherDetails.Transferor, new { @class = "form-control" })
                                        </div>
                                        <div class="col-sm-4">
                                            <label class="col-form-label font-weight-bolder">Date of transfer</label>
                                            <div class="input-group-prepend">
                                                <input type="text" class="form-control datetime" id="Dateof_Transfer" name="objOtherDetails.Dateof_Transfer" />
                                                <label class="input-group-text" for="inputDate"><i class="icon-calendar-alt-solid"></i></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row mt-3">
                                        <div class="col-sm-12 text-center">
                                            <a id="second-tab" data-toggle="tab" href="#MD" class="btn btn-dark btn-md ml-0 waves-effect waves-light btnPrevious" role="tab" aria-controls="second" aria-selected="false">Previous</a>
                                            <a id="third-tab" data-toggle="tab" href="#RRP" class="btn btn-primary btn-md ml-0 waves-effect waves-light btnNext" role="tab" aria-controls="third" aria-selected="false"> Next</a>
                                        </div>
                                    </div>

                                </div>
                                <div class="tab-pane fade" id="RRP" role="tabpanel" aria-labelledby="third-tab">

                                    <h5 class="text-brown font-weight-bold">3 Details of Rent/ Royalty / Dead Rent/ DMF /NMET amount paid in the month</h5>
                                    @Html.HiddenFor(m => m.objParticular.Flag2)
                                    @Html.Label("11 Particulars of area operated/Lease")
