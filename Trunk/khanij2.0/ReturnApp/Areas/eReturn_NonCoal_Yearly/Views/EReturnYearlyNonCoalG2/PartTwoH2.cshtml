                                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                                        <input type="number" value="@Model.objStaffWork.Whollyemployed1" name="objStaffWork.Whollyemployed1" id="objStaffWork_Whollyemployed1" onchange="TextChangeWhollyemployed()" class="form-control mb-lg-0 mb-3 StaffDetails" placeholder="Wholly employed">
                                                    </div>
                                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                                        <input type="number" value="@Model.objStaffWork.Partlyemployed1" name="objStaffWork.Partlyemployed1" id="objStaffWork_Partlyemployed1" class="form-control StaffDetails" onchange="TextChangeWhollyemployed()" placeholder="Partly employed">
                                                    </div>


                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xl-6">
                                            <div class="bg-light p-2 form-group border">
                                                <label class="col-form-label font-weight-bolder pt-0">(ii) Diploma Mining Engineer</label>
                                                <div class="row">

                                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                                        <input type="number" value="@Model.objStaffWork.Whollyemployed2" name="objStaffWork.Whollyemployed2" id="objStaffWork_Whollyemployed2" onchange="TextChangeWhollyemployed()" class="form-control mb-lg-0 mb-3 StaffDetails" placeholder="Wholly employed">
                                                    </div>
                                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                                        <input type="number" value="@Model.objStaffWork.Partlyemployed2" name="objStaffWork.Partlyemployed2" id="objStaffWork_Partlyemployed2" onchange="TextChangePartlyemployed()" class="form-control StaffDetails" placeholder="Partly employed">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xl-6">
                                            <div class="bg-light p-2 form-group border">
                                                <label class="col-form-label font-weight-bolder pt-0">(iii) Geologist</label>
                                                <div class="row">

                                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                                        <input type="number" value="@Model.objStaffWork.Whollyemployed3" name="objStaffWork.Whollyemployed3" id="objStaffWork_Whollyemployed3" onchange="TextChangeWhollyemployed()" class="form-control mb-lg-0 mb-3 StaffDetails" placeholder="Wholly employed">
                                                    </div>
                                                    <div class="col-lg-6 col-md-6 col-sm-12">
                                                        <input type="number" value="@Model.objStaffWork.Partlyemployed3" name="objStaffWork.Partlyemployed3" id="objStaffWork_Partlyemployed3" onchange="TextChangePartlyemployed()" class="form-control StaffDetails" placeholder="Partly employed">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xl-6">
                                            <div class="bg-light p-2 form-group border">
                                                <label class="col-form-label font-weight-bolder pt-0">(iv) Surveyor</label>
