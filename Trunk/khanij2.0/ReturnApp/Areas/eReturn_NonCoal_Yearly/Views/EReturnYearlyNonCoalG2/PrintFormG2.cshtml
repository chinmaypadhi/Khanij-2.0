@*// *********************************************************************************************************
    //  View Name                : PrintFormG2
    //  Desciption               : Print Yearly Return FORM G2
    //  Created By               : Akshaya Dalei
    //  Created On               : 18 Oct 2021
    // ********************************************************************************************************
*@
@model dynamic
@{
    ViewData["Title"] = "Print Yearly Return Form G2";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<html>
<head>
    <title>Yearly eReturn</title>
    <style>
        div, p, table {
            padding: 0px;
            margin: 0px;
        }


        .margin {
            margin: 25px;
        }

        .logo-title-border {
            border: 0.5px solid;
            margin-top: 20px;
            margin-bottom: 30px;
            margin-left: 10px;
            margin-right: 10px;
            padding: 10px;
            width: 226px;
        }

        .img-logo {
            margin-left: 40px;
            padding-top: 20px;
        }

        .barcode-title {
        }

        .size-a4 {
            width: 8.3in;
            height: 11.7in;
        }

        td {
            width: 50%;
            padding: 10px;
            margin: 10px;
        }
    </style>
</head>
<body class="margin">

    <section class="content-header">

        <ol class="breadcrumb">

            <li class="active">Print</li>
        </ol>
    </section>
    <br />
    @using (Html.BeginForm("", "", FormMethod.Post, new { id = "__G2AntiForgeryForm", @class = "form-horizontal bordered-row" }))
    {
        @if (Model.OtherDetails != null && !string.IsNullOrEmpty(Model.OtherDetails.DSCFilePath))
        {
            <span id="lblGenerate">Download FORM</span>
            <a target="_blank" class="btn btn-primary" href='@ViewBag.ServerPath@Model.OtherDetails.DSCFilePath'>Download</a>
        }
        else
        {
            <h1>
                Print
                <img src="~/img/1475779253_BT_printer.ico" style="height:40px;width:40px;cursor:pointer;" onclick="getPDF()" />
            </h1>
        }

        <input type="hidden" name="Year" value="@ViewBag.Year" id="Year" />
        <input type="hidden" name="OtherDetails.OtherDetails_Id" value="@Model.OtherDetails.OtherDetails_Id" id="OtherDetails_Id" />
        <input type="hidden" name="UserId" value="@ViewBag.UserId " id="Uid" />
        <div id="eReturnYearly" class="container-fluid  pdf-page size-a4" style="min-height: 17874px">
            <div>
                <p class="text-center"><b>FORM G2</b></p>
                <p class="text-center">[See rule 45(5) (c) (ii)]</p>
                <br />
                <p class="text-center">For the financial year <b>1st April,&nbsp; @ViewBag.begning&nbsp;&nbsp;&nbsp; to&nbsp;&nbsp;&nbsp; 31st March,&nbsp; @ViewBag.closing</b></p>
                <br />
                <p class="text-center">ANNUAL RETURN</p>
                <br />
                <p class="text-center">[To be used for minerals Copper, Gold, Lead, Pyrites, Tin, Tungsten and Zinc]</p>
                <br />
                <p class="text-left">To</p>
                <p class="text-left">&nbsp;&nbsp;&nbsp;&nbsp;(i) The Regional Controller of Mines</p>
                <p class="text-left">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Indian Bureau of Mines</p>
                <p class="text-left">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Raipur</b> &nbsp;&nbsp;Region,</p>
                <p class="text-left">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;PIN:&nbsp;&nbsp; <b>492001</b> </p>
                <p>
                    (Please address to Regional Controller of Mines in whose territorial jurisdiction the mines falls as notified from
                    time to time by the Controller General, Indian Bureau of Mines under rule 62 of the Mineral Conservation and
                    Development Rules, 1988)
                </p>
                <p class="text-left">&nbsp;&nbsp;&nbsp;&nbsp;(ii) The State Government</p>

            </div>
            @* PART – 1*@
            <div>
                <p class="text-center"><b>PART – I (General)</b></p>
                <div class="row">
                    <div class="col-sm-12">
                        <table class="" border="1" style="width: 100%;">
                            <tbody>
                                <tr>
                                    <td colspan="4">
                                        <p><b>1. Details of the Mine:</b></p>
                                    </td>

                                </tr>
                                <tr>
                                    <td>
                                        <p>
                                            (a) Registration number allotted by Indian Bureau of Mines<br />
                                            (to give registration number of the Lessee/ Owner)
                                        </p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.Registrationnumber</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>(b) Mine Code (allotted by Indian Bureau of Mines )</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.MINE_CODE</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>(c) Name of the Mineral</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.MineralName</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>(d) Name of Mine</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.MineName</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>
                                            (e) Name(s) of other mineral(s),
                                            if any, produced from the same mine
                                        </p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.OtherMineral</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="4">
                                        <p><b>2. Location of the Mine :</b></p>
                                    </td>

                                </tr>
                                <tr>
                                    <td>
                                        <p>Village</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.MineVillage</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>Post Office</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.OtherDetails.MinePostOffice</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>Tahsil/Taluk</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.MineTehsil</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>District</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.MineDistrict</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>State</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.MineState</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>PIN Code</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.MinePinCode</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <p>Fax no:&nbsp;&nbsp; @Model.OtherDetails.MineFaxNo</p>
                                    </td>
                                    <td colspan="2">
                                        <p>E-mail:&nbsp;&nbsp; @Model.MineDetails.MineEmailID</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <p>Phone no:&nbsp;&nbsp; @Model.MineDetails.MinePhoneNo</p>
                                    </td>
                                    <td colspan="2">
                                        <p>Mobile:&nbsp;&nbsp; @Model.MineDetails.MineMobileNo</p>
                                    </td>
                                </tr>
                                <tr class="page-break">
                                    <td colspan="4">
                                        <p><b>3. Name and address of Lessee/Owner (along with fax no. and e-mail):</b></p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>Name of Lessee/Owner</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.LesseeName</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>Address</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.LesseeAddress</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>District</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.LesseeDistrictName</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>State</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.LesseeStateName</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p>PIN Code</p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.MineDetails.LesseePinCode</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <p>Fax no:&nbsp;&nbsp; @Model.OtherDetails.LesseeFaxNo</p>
                                    </td>
                                    <td colspan="2">
                                        <p>E-mail:&nbsp;&nbsp; @Model.MineDetails.LesseeEmailID</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <p>Phone no:&nbsp;&nbsp; @Model.MineDetails.LesseePhoneNo</p>
                                    </td>
                                    <td colspan="2">
                                        <p>Mobile:&nbsp;&nbsp; @Model.MineDetails.LesseeMobileNo</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p><b>4. Registered Office of the Lessee:</b></p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.OtherDetails.RegisteredOffice</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p><b>5. Director in charge :</b></p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.OtherDetails.Director_in_charge</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p><b>6. Agent :</b></p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.OtherDetails.Agent</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p><b>7. Manager :</b></p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.OtherDetails.Manager</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p><b>8. Mining Engineer in charge:</b></p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.OtherDetails.MiningEngineer_in_charge</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p><b>9. Geologist in charge :</b></p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.OtherDetails.Geologist_in_charge</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <p><b>10. Transferor (previous owner), if any, and date of transfer:</b></p>
                                    </td>
                                    <td colspan="3">
                                        <p class="text-center">@Model.OtherDetails.Transferor</p>
                                        <br />
                                        <p class="text-center">@Model.OtherDetails.Dateof_Transfer</p>
                                    </td>
                                </tr>
                                <tr class="page-break">
                                    <td colspan="4">
                                        <p>
                                            <b>11. Particulars of area operated/Lease </b>
                                            <br />
                                            (Furnish information on items (i) to (vi) lease-wise in case mine workings cover more than one lease)
                                        </p>
                                    </td>
                                </tr>
                        </table>
                        <table id="tbl_AreaParticular" style="width: 100%" border="1">
                            <tr id="tr_Area"><td></td><td></td></tr>

                        </table>

                        <table style="width: 100%" border="1">
                            <tr>
                                <td>(vii) In case there is more than one mine in the same lease area, indicate name of mine and mineral produced</td>
                                <td colspan="3"><label id="More_mine_details"></label>&nbsp;&nbsp; and &nbsp;&nbsp; <label id="Mineral_produced"></label></td>
                            </tr>
                            <tr>
                                <td><b>12. Lease area (surface area) utilisation as at the end of year (hectares):</b></td>
                                <td style="width: 30%">Under forest</td>
                                <td style="width: 30%">Outside forest</td>
                                <td style="width: 40%">Total</td>
                            </tr>
                            <tr>
                                <td>(i) Already exploited & abandoned by opencast (O/C) mining</td>
                                <td style="width: 30%">@Model.LeaseArea.UnderForest1</td>
                                <td style="width: 30%">@Model.LeaseArea.Outsideforest1</td>
                                <td style="width: 40%">@Model.LeaseArea.Total1</td>
                            </tr>
                            <tr>
                                <td>(ii) Covered under current (O/C) Workings</td>
                                <td style="width: 30%">@Model.LeaseArea.UnderForest2</td>
                                <td style="width: 30%">@Model.LeaseArea.Outsideforest2</td>
                                <td style="width: 40%">@Model.LeaseArea.Total2</td>
                            </tr>
                            <tr>
                                <td>(iii) Reclaimed/rehabilitated</td>
                                <td style="width: 30%">@Model.LeaseArea.UnderForest3</td>
                                <td style="width: 30%">@Model.LeaseArea.Outsideforest3</td>
                                <td style="width: 40%">@Model.LeaseArea.Total3</td>
                            </tr>
                            <tr>
                                <td>(iv) Used for waste disposal</td>
                                <td style="width: 30%">@Model.LeaseArea.UnderForest4</td>
                                <td style="width: 30%">@Model.LeaseArea.Outsideforest4</td>
                                <td style="width: 40%">@Model.LeaseArea.Total4</td>
                            </tr>
                            <tr>
                                <td>(v) Occupied by plant, buildings, residential, welfare buildings & roads</td>
                                <td style="width: 30%">@Model.LeaseArea.UnderForest5</td>
                                <td style="width: 30%">@Model.LeaseArea.Outsideforest5</td>
                                <td style="width: 40%">@Model.LeaseArea.Total5</td>
                            </tr>
                            <tr>
                                <td>(vi) Used for any other purpose (specify)</td>
                                <td style="width: 30%">@Model.LeaseArea.UnderForest6</td>
                                <td style="width: 30%">@Model.LeaseArea.Outsideforest6</td>
                                <td style="width: 40%">@Model.LeaseArea.Total6</td>
                            </tr>
                            <tr>
                                <td>(vii) Work done under progressive mine closure plan during the year</td>
                                <td style="width: 30%">@Model.LeaseArea.UnderForest7</td>
                                <td style="width: 30%">@Model.LeaseArea.Outsideforest7</td>
                                <td style="width: 40%">@Model.LeaseArea.Total7</td>
                            </tr>
                            <tr>
                                <td><b>13. Ownership/exploiting Agency of the mine:(Public Sector/Private Sector/Joint Sector)</b></td>
                                <td colspan="3">@Model.LeaseArea.AgencyofMine</td>

                            </tr>

                        </table>

                    </div>
                </div>
            </div>
            <br />
            @* PART - 2*@
            <div>
                <p class="text-center page-break"><b>PART - II (Employment and Wages)</b></p>
                <br />
                <div class="row">
                    <div class="col-sm-12">
                        <table border="1" style="width: 100%;">
                            <tr>
                                <td colspan="3">1.Number of supervisory staff employed at the mine</td>
                            </tr>
                            <tr>
                                <td>Description</td>
                                <td>Wholly employed</td>
                                <td>Partly employed</td>
                            </tr>
                            <tr>
                                <td>(i) Graduate Mining Engineer</td>
                                <td>@Model.Staff.Whollyemployed1</td>
                                <td>@Model.Staff.Partlyemployed1</td>
                            </tr>
                            <tr>
                                <td>(ii) Diploma Mining Engineer</td>
                                <td>@Model.Staff.Whollyemployed2</td>
                                <td>@Model.Staff.Partlyemployed2</td>
                            </tr>
                            <tr>
                                <td>(iii) Geologist</td>
                                <td>@Model.Staff.Whollyemployed3</td>
                                <td>@Model.Staff.Partlyemployed3</td>
                            </tr>
                            <tr>
                                <td>(iv) Surveyor</td>
                                <td>@Model.Staff.Whollyemployed4</td>
                                <td>@Model.Staff.Partlyemployed4</td>
                            </tr>
                            <tr>
                                <td>(v) Other administrative and technical supervisory staff</td>
                                <td>@Model.Staff.Whollyemployed5</td>
                                <td>@Model.Staff.Partlyemployed5</td>
                            </tr>
                            <tr>
                                <td>Total:</td>
                                <td>@Model.Staff.Whollyemployed_Total</td>
                                <td>@Model.Staff.Partlyemployed_Total</td>
                            </tr>
                            <tr>
                                <td>2.(i) Number of days the mine worked:</td>
                                <td colspan="2">
                                    <label id="NoOfDaysWork"></label>
                                </td>
                            </tr>
                            <tr>
                                <td>(ii) No. of shifts per day:</td>
                                <td colspan="2">
                                    <label id="NoOfShif"></label>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <p>
                                        (iii) Indicate reasons for work stoppage in the mine during the year (due to strike, lockout, heavy
                                        rain, non-availability of labour, transport bottleneck, lack of demand, uneconomic operations,etc.)
                                        and the number of days of work stoppage for each of the factors separately .
                                    </p>
                                </td>
                                <td colspan="2">
                                    <table id="MiningWork" style="width: 100%;">
                                        <tr>
                                            <th style="width: 50%; text-align: center; border-bottom: 1px solid #080808;">Reasons</th>
                                            <th style="width: 50%; text-align: center; border-left: 1px solid #080808; border-bottom: 1px solid #080808;">No Of Days</th>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-sm-12">
                        <p class="text-left">3. Employment and salary/wages paid #:</p>
                        <br />
                        <table border="1" style="width: 100%;">
                            <tr>
                                <td colspan="9">
                                    Maximum number of persons employed on any one day during the year:<br />
                                    (i) In workings below ground on (date)...<b>@Model.SalaryWages.WorkingBelowGrounddate_str</b> (a) ( number).....<b>@Model.SalaryWages.WorkingBelowNo</b><br />
                                    (ii) In all in the mine on (date)...<b>@Model.SalaryWages.WorkingOnMinedate_str</b> (a) ( number).....<b>@Model.SalaryWages.WorkingOnMineNo</b>
                                </td>
                            </tr>
                            <tr>
                                <td rowspan="2">Classification</td>
                                <td colspan="3">Total number of man days worked during the year</td>
                                <td rowspan="2">No. of days worked during the year</td>
                                <td colspan="3">Average daily number of persons employed</td>
                                <td rowspan="2">Total Wages/Salary for the year (₹)</td>
                            </tr>
                            <tr>
                                <td>Direct</td>
                                <td>Contract</td>
                                <td>Total</td>
                                <td>Male</td>
                                <td>Female</td>
                                <td>Total</td>
                            </tr>
                            <tr>

                                <td style="width: 20%">(1)</td>
                                <td style="width: 10%">2(A)</td>
                                <td style="width: 10%">2(B)</td>
                                <td style="width: 10%">2(C)</td>
                                <td style="width: 10%">(3)</td>
                                <td style="width: 10%">4(A)</td>
                                <td style="width: 10%">4(B)</td>
                                <td style="width: 10%">4(C)</td>
                                <td style="width: 10%">(5)</td>

                            </tr>
                            <tr>
                                <td>Below Ground</td>
                                <td>@Model.SalaryWages.BG_Direct</td>
                                <td>@Model.SalaryWages.BG_Contract</td>
                                <td>@Model.SalaryWages.BG_Total</td>
                                <td>@Model.SalaryWages.BG_NoOFDaysWork</td>
                                <td>@Model.SalaryWages.BG_Male</td>
                                <td>@Model.SalaryWages.BG_Female</td>
                                <td>@Model.SalaryWages.BG_Average_Total</td>
                                <td>@Model.SalaryWages.BG_TotalWages</td>
                            </tr>
                            <tr>
                                <td>Opencast</td>
                                <td>@Model.SalaryWages.OC_Direct</td>
                                <td>@Model.SalaryWages.OC_Contract</td>
                                <td>@Model.SalaryWages.OC_Total</td>
                                <td>@Model.SalaryWages.OC_NoOFDaysWork</td>
                                <td>@Model.SalaryWages.OC_Male</td>
                                <td>@Model.SalaryWages.OC_Female</td>
                                <td>@Model.SalaryWages.OC_Average_Total</td>
                                <td>@Model.SalaryWages.OC_TotalWages</td>
                            </tr>
                            <tr>
                                <td>Above Ground</td>
                                <td>@Model.SalaryWages.AG_Direct</td>
                                <td>@Model.SalaryWages.AG_Contract</td>
                                <td>@Model.SalaryWages.AG_Total</td>
                                <td>@Model.SalaryWages.AG_NoOFDaysWork</td>
                                <td>@Model.SalaryWages.AG_Male</td>
                                <td>@Model.SalaryWages.AG_Female</td>
                                <td>@Model.SalaryWages.AG_Average_Total</td>
                                <td>@Model.SalaryWages.AG_TotalWages</td>
                            </tr>
                            <tr>
                                <td>Total:</td>
                                <td>@Model.SalaryWages.Total_Direct</td>
                                <td>@Model.SalaryWages.Total_Contract</td>
                                <td>@Model.SalaryWages.Total_Total</td>
                                <td>@Model.SalaryWages.Total_NoOFDaysWork</td>
                                <td>@Model.SalaryWages.Total_Male</td>
                                <td>@Model.SalaryWages.Total_Female</td>
                                <td>@Model.SalaryWages.Total_Average_Total</td>
                                <td>@Model.SalaryWages.Total_TotalWages</td>
                            </tr>
                        </table>
                        <p class="text-left">Note : To include all employees exclusive to the mine and attached factory, workshop or mineral dressing plant at the mine site</p>
                    </div>
                </div>
            </div>
            <br />
            @* PART-2 A*@
            <div>
                <p class="text-center page-break"><b>PART-II A (Capital Structure)</b></p>
                <br />
                <div class="row">
                    <div class="col-sm-12">
                        <table border="1" style="width: 100%;">
                            <tr>
                                <td colspan="7">
                                    1. Value of Fixed Assets* (₹……@Model.CapitalStucture.ValueofFixedAssets)
                                    (in respect of the mine, beneficiation plant, mine work-shop, power and water installation)
                                    In case this information is furnished as combined information in another mine's return please specify Mine Code/Mine
                                    Name:@Model.CapitalStucture.OtherMineCode
                                </td>
                            </tr>
                            <tr>
                                <td>"Description"</td>
                                <td>At the beginning of the year(₹)</td>
                                <td>Additions during the Year(₹)")</td>
                                <td>Sold or discarded during the year(₹)</td>
                                <td>Depreciation during the year(₹)</td>
                                <td>Net closing Balance (₹) (2+3)-(4+5)</td>
                                <td>Estimated market value**(₹)</td>
                            </tr>
