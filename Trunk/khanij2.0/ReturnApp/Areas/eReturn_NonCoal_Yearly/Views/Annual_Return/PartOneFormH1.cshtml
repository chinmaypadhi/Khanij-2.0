                                                <tr>
                                                    <th style="border-color: black;" rowspan="2">Lease number allotted by the State Government</th>
                                                    <th style="border-color: black;" colspan="3">Area under lease (hectares):</th>
                                                    <th style="border-color: black;" rowspan="2">Date of execution of mining lease deed</th>
                                                    <th style="border-color: black;" rowspan="2">Period of lease</th>
                                                    <th style="border-color: black;" colspan="3">Area for which surface rights are held (hectares)</th>
                                                    <th style="border-color: black;" colspan="2">Date and period of renewal (if applicable)</th>

                                                </tr>
                                                <tr>

                                                    <th>Under Forest</th>
                                                    <th>Outside Forest</th>
                                                    <th>Total</th>
                                                    <th>Under Forest</th>
                                                    <th>Outside Forest</th>
                                                    <th>Total</th>
                                                    <th>Date (if applicable)</th>
                                                    <th>Period of renewal</th>
                                                </tr>
                                            </thead>


                                            <tr id="tr_MoreThen_OneLease">
                                                <td>
                                                    @Html.HiddenFor(m => m.objParticular.Particular_Id)
                                                    @Html.TextBoxFor(m => m.objParticular.Lease_number, new { @class = "form-control" })
                                                </td>
                                                <td>
                                                    @Html.TextBoxFor(m => m.objParticular.underlease_UnderForest, new { @class = "form-control" })
                                                </td>
                                                <td>

                                                    @Html.TextBoxFor(m => m.objParticular.underlease_OutsideForest, new { @class = "form-control" })
                                                </td>
                                                <td>

                                                    @Html.TextBoxFor(m => m.objParticular.underlease_Total, new { @class = "form-control" })
                                                </td>
                                                <td>

                                                    @Html.TextBoxFor(m => m.objParticular.Date_of_execution, new { @class = "form-control" })
                                                </td>
                                                <td>

                                                    @Html.TextBoxFor(m => m.objParticular.Periodoflease, new { @class = "form-control" })
                                                </td>
                                                <td>

                                                    @Html.TextBoxFor(m => m.objParticular.Surfacerights_UnderForest, new { @class = "form-control" })
                                                </td>
                                                <td>

                                                    @Html.TextBoxFor(m => m.objParticular.Surfacerights_OutsideForest, new { @class = "form-control" })
                                                </td>
                                                <td>

                                                    @Html.TextBoxFor(m => m.objParticular.Surfacerights_Total, new { @class = "form-control" })
                                                </td>
                                                <td>

                                                    @Html.TextBoxFor(m => m.objParticular.RenewalDate)
                                                </td>
                                                <td>

                                                    @Html.TextBoxFor(m => m.objParticular.RenewalPeriod, new { @class = "form-control" })
                                                </td>
                                            </tr>

                                        </table>
                                    </div>

                                    <div class="">
                                        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                            @Html.Label("(vii) In case there is more than one mine in the same lease area, indicate name of mine")
                                            @Html.TextBoxFor(m => m.objParticular.More_mine_details, new { @class = "form-control" })
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                            @Html.Label("(vii) In case there is more than one mine in the same lease area, indicate name of mineral produced")
                                            @Html.TextBoxFor(m => m.objParticular.Mineral_produced, new { @class = "form-control" })
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12"></div>
                                    </div>

                                    <div class="row mt-3">
                                        <div class="col-sm-12 text-center">
                                            <a id="third-tab" data-toggle="tab" href="#LAD" class="btn btn-dark btn-md ml-0 waves-effect waves-light btnPrevious" role="tab" aria-controls="third" aria-selected="false"> Previous</a>
                                            <a id="third-tab" data-toggle="tab" href="#DWM" class="btn btn-primary btn-md ml-0 waves-effect waves-light btnNext" role="tab" aria-controls="third" aria-selected="false"> Next</a>

                                        </div>
