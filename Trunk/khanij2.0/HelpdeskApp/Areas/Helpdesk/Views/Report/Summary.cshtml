@model HelpDeskEF.RaiseTicket
@{
    ViewData["Title"] = "Summary";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}
<style>
    .summary-details {
        height: 256px;
        overflow-y: auto;
        padding: 15px;
    }


        .summary-details ul li {
            list-style-type: none;
            padding: 10px 0;
            font-size: 15px;
            display: flex;
            justify-content: space-between;
            border-bottom: 1px dashed var(--input-border);
            color: var(--font-color);
        }

            .summary-details ul li a {
                font-size: 20px;
                font-weight: 700
            }

    .report-summary-card {
        border: 1px solid var(--input-border);
        margin-bottom: 1rem;
        border-radius: 5px;
        overflow: hidden;
    }

    .report-summary-card-header {
        padding: 10px 15px;
        border-bottom: 1px solid var(--input-border);
        color: #4e4e4e;
    }

    .report-summary-card h5 {
        font-size: 18px;
        margin: 0;
        font-weight: 600;
        color: var(--gray);
    }
</style>
<script>
    $(document).ready(function () {
        loadNavigation('Add', 'glRoleRes', 'plUser', 'tl', 'Helpdesk', 'Summary', ' ');
    });
</script>
<section class="wrapper main-wrapper row">
    <div class="col-12">
        @Html.Partial("_CMSnavigation")
        <!-- MAIN CONTENT AREA STARTS -->
        <div class="row">
            <div class="col-12">
                <div class="main-tab">
                    <ul class="nav nav-tabs">
                        <li class="nav-item">
                            <a class="nav-link active" href="javascript:void(0);">
                                Summary Report
                            </a>
                        </li>
                    </ul>
                    @Html.Partial("_CMSutil")
                </div>
                <section class="box form-container">
                    <div class="searchform px-3 py-3">
                        <div class="row">
                            <div class="col-sm-12">
                                @*<form asp-action="Summary" asp-controller="Report" method="post" enctype="multipart/form-data">*@
                                <div class="row align-items-lg-center">
                                    <label class="col-lg-1 col-md-4 col-sm-12 col-form-label">From Date</label>
                                    <div class="col-lg-3 col-md-6 col-sm-12 mb-lg-0 mb-3">
                                        <div class="input-group">
                                            <input type="text" class="form-control datetime" name="fromdate" id="fromdate" />
                                            <div class="input-group-prepend">
                                                <label class="input-group-text" for="fromdate"><i class="icon-calendar-alt-solid"></i></label>
                                            </div>
                                        </div>
                                    </div>
                                    <label class="col-lg-1 col-md-4 col-sm-12 col-form-label">To Date</label>
                                    <div class="col-lg-3 col-md-6 col-sm-12 mb-lg-0 mb-3">
