@model MasterEF.IBMDetails
@{
    ViewData["Title"] = "IBMDetails";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}
<section class="wrapper main-wrapper row">
    <div class="col-12">
        <!-- MAIN CONTENT AREA STARTS -->
        @Html.Partial("_CMSnavigation")
        <div class="row">
            <div class="col-12">
                <div class="main-tab">
                    <ul class="nav nav-tabs">
                        <li class="nav-item">
                            <a class="nav-link" href="~/Licensee/UserInformationLiensee/IBMDetails">
                                Add
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" href="javascript:void(0);">
                                View
                            </a>
                        </li>
                    </ul>
                    @Html.Partial("_CMSutil")
                </div>
                <section class="box form-container">
                    <div class="content-body">
                        <div class="message-heading">
                            <h5 class="font-weight-bold text-brown">IBM Details</h5>
                            <div>
                                <a href="javascript:void(0);" class="btn-dark btn-sm log-btn" data-val="collapsefillter1">Log Report<i class="icon-angle-down-solid"></i></a>
                                <a href="~/Licensee/UserInformationLiensee/IBMDetails" class="btn-success btn-sm ml-3" data-original-title="Edit"><i class="icon-pencil-alt-solid"></i></a>
                            </div>
                        </div>
                        <div class="message-highlighter">
                            <div class="row">
                                <label class="col-lg-2 col-md-4 col-sm-12 col-form-label">IBM_NUMBER</label>
                                <div class="col-lg-2 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext">
                                        <span class="colon">:</span>@Model.IBM_NUMBER
                                    </label>
                                </div>
                                <label class="col-lg-2 col-md-4 col-sm-12 col-form-label">IBM Registration Number</label>
                                <div class="col-lg-2 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext">
                                        <span class="colon">:</span>@Model.IBM_APPLICATION_NUMBER
                                    </label>
                                </div>
                                <label class="col-lg-2 col-md-4 col-sm-12 col-form-label">IBM Registration Date</label>
                                <div class="col-lg-2 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.IBM_APPLICATON_DATE</label>
                                </div>

                                <label class="col-lg-2 col-md-4 col-sm-12 col-form-label">Registration Copy</label>
                                <div class="col-lg-2 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext">
                                        <span class="colon">:</span>File
                                        @if (Model.FILE_IBM_REGISTRATION_FORM != null && Model.FILE_IBM_REGISTRATION_FORM != "")
                                        {
                                            @*<a href="~/UploadMisDocs/CTO/2820211734252_TimeSheet Policy_May21.pdf" title="" class="ml-2" data-original-title="Download Execution Deed" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a>*@
                                            <a href="~/@Model.IBM_REGISTRATION_FORM_PATH" title="" class="ml-2" data-original-title="Download Registration Copy" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a>
                                        }
                                    </label>
                                </div>

                                <label class="col-lg-2 col-md-4 col-sm-12 col-form-label">Remarks</label>
                                <div class="col-lg-2 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.Remarks</label>
                                </div>
                            </div>
                            <ul class='timeline custom-scroll log-box' id='collapsefillter1'>
                                @Html.Raw((String)ViewBag.tableLA)
                            </ul>
                        </div>

                    </div>
                </section>
            </div>
        </div>
    </div>
</section>