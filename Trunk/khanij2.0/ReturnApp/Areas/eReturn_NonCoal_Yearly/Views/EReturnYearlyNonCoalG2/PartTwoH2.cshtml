                        $("#third").addClass("tab-pane fade show active");
                        alert("This field is required.");
                        OC[j].focus();
                        return false
                        break;
                    }
                }
            }
        }
        for (var i = 0; i < AG.length; i++) {
            if (AG[i].value > 0 && i <= AG.length) {
                for (var j = 0; j < AG.length; j++) {
                    if (AG[j].value == "") {
                        $("#five").removeClass("active");
                        $("#third").addClass("tab-pane fade show active");
                        alert("This field is required.");
                        AG[j].focus();
                        return false
                        break;
                    }
                }
            }
        }
    }

