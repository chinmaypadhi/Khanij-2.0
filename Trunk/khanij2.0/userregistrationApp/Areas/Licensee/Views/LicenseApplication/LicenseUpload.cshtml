}
@model userregistrationEF.LicenseeDocUploadModel
@{
    if (ViewBag.AckMessage == "1")
    {
        <script>
            alert("All Files are uploaded successfully.");
        </script>
    }
    else if (ViewBag.AckMessage == "2")
    {
        <script>
            alert(" File uploading process failed ! Please Try Again.");
        </script>
    }
}
