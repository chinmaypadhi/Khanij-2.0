                                <div class="col-lg-3 col-md-8 col-sm-12 divLabEstablished">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.IsLabEstablished</label>
                                </div>
                                @if (Model.IsLabEstablished == "Yes" && profile.MineralTypeName != "Minor Mineral")
                                {
                                    <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Upload copy of Inspection report by Officials(RH,DGM)</label>
                                    <div class="col-lg-3 col-md-8 col-sm-12">
                                        @if (Model.LabExemption_PATH != null)
                                        {
                                            <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/LabInstalled/@Model.LabExemption_PATH' title="" class="ml-2" data-original-title="Download Lab Installed Document" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                        }
                                    </div>
                                }
                                else if (Model.IsLabEstablished == "No" && profile.MineralTypeName != "Minor Mineral")
                                {
                                    <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Upload order copy of exemption by DGM / MRD</label>
                                    <div class="col-lg-3 col-md-8 col-sm-12">
                                        @if (Model.LabExemption_PATH != null)
                                        {
                                            <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/LabInstalled/@Model.LabExemption_PATH' title="" class="ml-2" data-original-title="Download Lab Installed Document" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                        }
                                    </div>
                                }
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Application Type</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.IsAuctionType</label>
                                </div>
                                @if (Model.AuctionTypeId == 2)
                                {
                                    <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divAuctionDiv">Monthly Periodic Amount/Auction Money(in <i class="fas fa-rupee-sign"></i>)</label>
                                    <div class="col-lg-3 col-md-8 col-sm-12 divAuctionDiv">
                                        <label class="form-control-plaintext"><span class="colon">:</span>@Model.MonthlyPeriodicAmt</label>
                                    </div>
                                    <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divBidPremium">Bid Premium(%)</label>
                                    <div class="col-lg-3 col-md-8 col-sm-12 divBidPremium">
                                        <label class="form-control-plaintext"><span class="colon">:</span>@Model.BidPremium</label>
                                    </div>
                                }
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Storage / Crusher</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.StorageCrusher</label>
                                </div>
                                @if (Model.StorageCrusher == "Yes")
                                {
                                    <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Crusher Installed</label>
                                    <div class="col-lg-3 col-md-8 col-sm-12">
                                        <label class="form-control-plaintext"><span class="colon">:</span>@Model.CrusherInstalled</label>
                                    </div>
                                    @if (Model.CrusherInstalled == "No")
                                    {
                                        <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Storage/Crusher License Copy</label>
                                        <div class="col-lg-3 col-md-8 col-sm-12">
                                            @if (Model.StorageCrusherAttachment_FILE != null)
                                            {
                                                <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/Minor_Mineral/@Model.StorageCrusherAttachment_FILE' title="" class="ml-2" data-original-title="Download Storage Crusher Document" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                            }
                                        </div>
                                    }
                                }
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divCoal">Adjust E & I CESS</label>
                                <div class="col-lg-3 col-md-8 col-sm-12 divCoal">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.AdjustCess</label>
                                </div>
                                @if (Model.AdjustCess == "Yes" && profile.MineralTypeName == "COAL")
                                {
                                    <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Order of</label>
                                    <div class="col-lg-3 col-md-8 col-sm-12">
                                        <label class="form-control-plaintext"><span class="colon">:</span>@Model.OrderOf</label>
                                    </div>
                                    <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Order No</label>
                                    <div class="col-lg-3 col-md-8 col-sm-12">
                                        @if (Model.OrderAttachment_File != null)
                                        {
                                            <label class="form-control-plaintext"><span class="colon">:</span>@(Model.OrderNo) <a href='~/Upload/Lessee/APPLICATION/APPLICATION_FORM/@Model.OrderAttachment_File' title="" class="ml-2" data-original-title="Download Attached Order Document" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                        }
                                    </div>
                                    <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Order Date</label>
                                    <div class="col-lg-3 col-md-8 col-sm-12">
                                        <label class="form-control-plaintext"><span class="colon">:</span>@Model.OrderDate</label>
                                    </div>
                                }
                            </div>
                            <ul class="timeline custom-scroll log-box" id="collapsefillter1">
                                @Html.Raw((String)ViewBag.tableLA)
                            </ul>
                        </div>
                        <div class="message-heading">
                            <h5 class="font-weight-bold text-brown">Attachments Information</h5>
                            <div>
                                <a href="javascript:void(0);" class="btn-dark btn-sm log-btn" data-val="collapsefillter2">Log Report <i class="icon-angle-down-solid"></i></a>
                                <a href="@CustomQueryStringHelper.EncryptString("LesseeProfile","Add","LesseeDetails",new {id=Model.CREATED_BY })" class="btn-success btn-sm ml-3" data-original-title="Edit"><i class="icon-pencil-alt-solid"></i></a>
                            </div>
                        </div>
                        <div class="message-highlighter">
                            <div class="row">
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Power of Attorney</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <span class="colon">:</span>@Model.POWER_OF_ATORNY_FILE_NAME
                                    @if (Model.POWER_OF_ATORNY_FILE_NAME != null && Model.POWER_OF_ATORNY_FILE_NAME != "" && Model.POWER_OF_ATORNY_FILE_NAME != "--")
                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/POWER_OF_ATORNY/@Model.POWER_OF_ATORNY_FILE_NAME' title="" class="ml-2" data-original-title="Download" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
