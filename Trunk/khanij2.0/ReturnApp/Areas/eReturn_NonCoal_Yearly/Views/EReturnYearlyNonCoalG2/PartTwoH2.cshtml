    function ValidatePartII() {
        for (var i = 0; i < 2; i++) {
            if (ValidateStaffDetails() == false) {
                return false;
            }
            else if (ValidateWorkDetails() == false) {
                return false;
            }
            else if (ValidateSalaryWages() == false) {
                return false;
            }
            else if (ValidateCapitalStructure() == false) {
                return false;
            }
            else if (ValidateSource() == false) {
                return false;
            }
            else { return true; }
        }
    }
