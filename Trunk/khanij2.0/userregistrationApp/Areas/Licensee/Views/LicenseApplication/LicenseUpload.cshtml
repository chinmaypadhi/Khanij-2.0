    function checkFields() {
        var valid = 1;


        if ($('#North').val() == "") {
            alert("Enter North Description");
            valid = 0;
            return false;
        }

        if ($('#East').val() == "") {
            alert("Enter East Description !");
            valid = 0;
            return false;
        }

