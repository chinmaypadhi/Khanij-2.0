<script>
    /* backMe = "yes"*/

    $(document).ready(function () {
        loadNavigation('Profile', 'glTMng', 'plpaymenthead', 'tl', 'EndUser', 'View Profile', ' ');
        onRadioChange();
    });
</script>

    <script type="text/javascript">
        $(window).on('load', function () {
            $('#Notice_model').modal('show');
    });
    function onRadioChange() {
        var SelectedRadioVal = $("#hdnUserType").val()

        if (SelectedRadioVal == 'Firm') {
            $('#divCompanyFirmAssociation').show();
            $("#company_toggle").hide();
            $("#firm_toggle").show();
            $("#individual_toggle").hide();
            $("#individual_toggle1").hide();
        }
        else if (SelectedRadioVal == 'Company') {
            $('#divCompanyFirmAssociation').show();
            $("#firm_toggle").hide();
            $("#company_toggle").show();
            $("#individual_toggle").hide();
            $("#individual_toggle1").hide();
        }
        else if (SelectedRadioVal == 'Association') {
            $("#firm_toggle").hide();
            $("#company_toggle").hide();
            $('#divCompanyFirmAssociation').show();
            $("#individual_toggle").hide();
            $("#individual_toggle1").hide();
        }
        else {
            $('#divCompanyFirmAssociation').hide();
            $("#firm_toggle").hide();
            $("#company_toggle").hide();
            $("#individual_toggle").show();
            $("#individual_toggle1").show();
        }
    }
</script>

