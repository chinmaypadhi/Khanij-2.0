    function Validate2() {
        var Iss = false;
        //Module 1
        //Total Production

        if (document.getElementById("SR_SS1_Systematic_Sustainable_Sub_Type").value == "") {
            swal("", "Please enter Total Total Production Mineral wise during Reporting Year (RY) --> Approved Quantity(tones)", "warning");
