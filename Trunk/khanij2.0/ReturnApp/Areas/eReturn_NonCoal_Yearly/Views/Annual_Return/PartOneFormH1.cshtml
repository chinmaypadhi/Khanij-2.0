        var UnderForest, OutsideForest, Total;

        for (var i = 0; i < Surfacerights_UnderForest.length; i++) {
            Surfacerights_UnderForest[i].value = Surfacerights_UnderForest[i].value || 0;
            Surfacerights_OutsideForest[i].value = Surfacerights_OutsideForest[i].value || 0;
            Surfacerights_Total[i].value = parseFloat(Surfacerights_UnderForest[i].value) + parseFloat(Surfacerights_OutsideForest[i].value);
            if (Surfacerights_UnderForest[i].value == 0) {
                Surfacerights_UnderForest[i].value = "";
            }
            if (Surfacerights_OutsideForest[i].value == 0) {
                Surfacerights_OutsideForest[i].value = "";
            }
            if (Surfacerights_Total[i].value == 0) {
                Surfacerights_Total[i].value = "";
            }
        }

    }


    function ValidateTextBox(e) {
        var regex = new RegExp("^[0-9.]+$");
        //var regex = new RegExp("^[^,]+$");
        var str = String.fromCharCode(!e.keyCode ? e.which : e.keyCode);
        if (regex.test(str)) {
            return true;
        }
        return false;
    }

