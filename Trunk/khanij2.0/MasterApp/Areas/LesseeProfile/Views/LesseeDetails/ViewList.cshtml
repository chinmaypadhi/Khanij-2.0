                                    <div class="col-lg-3 col-md-8 col-sm-12">
                                        @if (Model.WBExemption_PATH != null)
                                        {
                                            <label class="form-control-plaintext"><span class="colon">:</span> <a href='~/Upload/Lessee/APPLICATION/WBInstalled/@Model.WBExemption_FILE' title="" class="ml-2" data-original-title="Download Document" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                        }
                                    </div>
