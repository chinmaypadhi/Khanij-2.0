@*// *********************************************************************************************************
    //  View Name                : Profile
    //  Desciption               : Bind Tailingdam profile details
    //  Created By               : Lingaraj Dalai
    //  Created On               : 02 June 2022
    // ********************************************************************************************************
*@
@model userregistrationEF.cls_TailingDam;
@using userregistrationEF;
@using userregistrationApp.Helper;
@using  Microsoft.AspNetCore.Http;
@inject IHttpContextAccessor HttpContextAccessor
@{
    ViewData["Title"] = "Profile";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}
@if (TempData["Message"] != null)
{
<script>
    var vv = '@TempData["Message"]';
    if (vv == 0) {
        swal("", "Record already existed with either applicant name or mobile number or email address  .", "warning");
    }
    else if (vv == 1) {
        swal("", "Profile updated successfully.", "success");
    }
    else if (vv == 2) {
        swal("", "Something Errror occured. Please Try After Sometime!!", "error");
    }
</script>
}
<script>
    backMe = "yes"
    $(document).ready(function () {
        debugger;
        loadNavigation('Profile', 'glUIS', 'pllesseprof', 'tl', 'Tailing Dam', 'Profile', ' ');
    });
</script>
<section class="wrapper main-wrapper row">
    <div class="col-12">
        <!-- MAIN CONTENT AREA STARTS -->
        @Html.Partial("_CMSnavigation")
        <div class="row">
            <div class="col-12">
                <div class="main-tab">
                    <ul class="nav nav-tabs">
                        <li class="nav-item">
                            <a class="nav-link" id="navlinkAdd" href="~/Contractor/Contractor/EditProfile">
                                Add
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" href="javascript:void(0);">
                                View
                            </a>
                        </li>
                    </ul>
                    @Html.Partial("_CMSutil")
                </div>
                <section class="box form-container">
                    <div class="content-body">
                        <div class="message-heading">
                            <h5 class="font-weight-bold text-brown">Owner Details</h5>
                            <div>
                                <a href="@CustomQueryStringHelper.EncryptString("Contractor","EditProfile","Contractor",new {id=Model.UserId })" class="btn-success btn-sm ml-3" data-original-title="Edit"><i class="icon-pencil-alt-solid"></i></a>
                            </div>
                        </div>
                        <div class="message-highlighter">
                            <div class="row">
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divRevRecovery">Name of the Authorised Person</label>
                                <div class="col-lg-9 col-md-8 col-sm-12 divRevRecovery">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.OwnerName</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divRevRecovery">Designation</label>
                                <div class="col-lg-9 col-md-8 col-sm-12 divRevRecovery">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.Designation</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Company</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.CompanyName</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Address</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.Address</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divRevRecovery">Mobile Number</label>
                                <div class="col-lg-9 col-md-8 col-sm-12 divRevRecovery">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.MobileNo</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divRevRecovery">E-mail ID</label>
                                <div class="col-lg-9 col-md-8 col-sm-12 divRevRecovery">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.EmailID</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Order No.</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.OrderNo</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Order Date</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.OrderDate</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Period Validity From</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.PeriodValidityFrom</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Period Validity To</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.PeriodValidityTo</label>
                                </div>
                            </div>
                        </div>
                        <div class="message-heading">
                            <h5 class="font-weight-bold text-brown">Site Details</h5>
                            <div>
                                <a href="@CustomQueryStringHelper.EncryptString("Contractor","EditProfile","Contractor",new {id=Model.UserId })" class="btn-success btn-sm ml-3" data-original-title="Edit"><i class="icon-pencil-alt-solid"></i></a>
                            </div>
                        </div>
                        <div class="message-highlighter">
                            <div class="row">
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divRevRecovery">District</label>
                                <div class="col-lg-9 col-md-8 col-sm-12 divRevRecovery">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.DistrictName</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divRevRecovery">Tehsil</label>
                                <div class="col-lg-9 col-md-8 col-sm-12 divRevRecovery">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.TehsilName</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Village</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.VillageName</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Area (In Hectare)</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.Area</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divRevRecovery">Type of Site</label>
                                <div class="col-lg-9 col-md-8 col-sm-12 divRevRecovery">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.TypeOfSite</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label divRevRecovery">Mineral Type</label>
                                <div class="col-lg-9 col-md-8 col-sm-12 divRevRecovery">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.MineralTypeName</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Mineral Name</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.MineralName</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Mineral Form</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.MineralForm</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Mineral Grade</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.MineralGrade</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Current Stock ( As on date )(@Model.UnitName)</label>
                                <div class="col-lg-9 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.CurrentStock</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>