    function validateTableA() {
        var A1 = $("#objEmployment_BG_Overman_2A").val();
        var B1 = $("#objEmployment_BG_Mine_Loader_2A").val();
        var C1 = $("#objEmployment_BG_Others_2A").val();
        var D1 = $("#objEmployment_OC_Overman_2A").val()
        var E1 = $("#objEmployment_OC_Mine_Loader_2A").val();
        var F1 = $("#objEmployment_OC_Others_2A").val();
        var G1 = $("#objEmployment_CS_staff_2A").val();
        var H1 = $("#objEmployment_Workers_2A").val();
        var I1 = $("#objEmployment_Others_2A").val();

        if ($("#objMine_Certify").prop('checked') == false) {
            alert('Please Checked Certify Statement!')
            $("#seven").removeClass("active");
            $("#first").addClass("tab-pane fade show active");
            $("#objMine_Certify").focus();
            return false;
        }
        if ($('#objEmployment_Belowground_onDatestr').val() == "") {
            alert('Please select workings below ground on.');
            $("#seven").removeClass("active");
            $("#second").addClass("tab-pane fade show active");
            $('#objEmployment_Belowground_onDatestr').focus();
            return false;
        }
        else if ($('#objEmployment_Belowground_No_Employee').val() == "") {
            alert('Please enter no of employee.');
            $("#seven").removeClass("active");
            $("#second").addClass("tab-pane fade show active");
            $('#objEmployment_Belowground_No_Employee').focus();
            return false;
        }
        else if ($('#objEmployment_Mine_onDatestr').val() == "") {
            alert('Please select all in the mine on.');
            $("#seven").removeClass("active");
            $("#second").addClass("tab-pane fade show active");
            $('#objEmployment_Mine_onDatestr').focus();
            return false;
        }
        else if ($('#objEmployment_Mine_No_Employee').val() == "") {
            alert('Please enter no of employee.');
            $("#seven").removeClass("active");
            $("#second").addClass("tab-pane fade show active");
            $('#objEmployment_Mine_No_Employee').focus();
            return false;
        }
        else if (A1 == "" && B1 == "" && C1 == "" && D1 == "" && E1 == "" && F1 == "" && H1 == "" && I1 == "") {
            alert('Please enter Total number of mandays worked during the year (b)');
            $("#seven").removeClass("active");
            $("#second").addClass("tab-pane fade show active");
            return false;
        }


        else {

            // If bellow flags becomes false it checkes for is field is required
            var BG_Flag = true;
            var OC_Flag = true;
            var AG_Flag = true;

            //If count becomes 27 means above flag becomes true and form will be validate
            var BG_Count = 0;
            var OC_Count = 0;
            var AG_Count = 0;
            //var txtbox = document.querySelectorAll(".BG,.OC,.AG");
            var BG = document.getElementsByClassName('BG');
            var OC = document.getElementsByClassName('OC');
            var AG = document.getElementsByClassName('AG');

            var BG = document.getElementsByClassName('BG');
            for (var i = 0; i < BG.length; i++) {
                if (parseInt(BG[i].value) > 0) {
                    BG_Flag = false;
                    BG_Count += 1;
                }
                if (parseInt(OC[i].value) > 0) {
                    OC_Flag = false;
                    OC_Count += 1;
                }
                if (parseInt(AG[i].value) > 0) {
                    AG_Flag = false;
                    AG_Count += 1;
                }
                if (BG_Count == BG.length) {
                    BG_Flag = true;
                }
                if (OC_Count == OC.length) {
                    OC_Flag = true;
                }
                if (AG_Count == AG.length) {
                    AG_Flag = true;
                }
            }
            if (BG_Flag == false) {
                for (var i = 0; i < BG.length; i++) {
                    if (BG[i].value == "") {
                        $("#seven").removeClass("active");
                        $("#second").addClass("tab-pane fade show active");
                        $(BG[i]).focus();
                        alert('This fields is required.')
                        return false;
                    }
                }
            }
            else if (OC_Flag == false) {
                for (var i = 0; i < OC.length; i++) {
                    if (OC[i].value == "") {
                        $("#seven").removeClass("active");
                        $("#second").addClass("tab-pane fade show active");
                        $(OC[i]).focus();
                        alert('This fields is required.')
                        return false;
                    }
                }
            }
            else if (AG_Flag == false) {
                for (var i = 0; i < AG.length; i++) {
                    if (AG[i].value == "") {
                        $("#seven").removeClass("active");
                        $("#second").addClass("tab-pane fade show active");
                        $(AG[i]).focus();
                        alert('This fields is required.')
                        return false;
                    }
                }
            }
            else {
                return true;
            }
        }
    }

