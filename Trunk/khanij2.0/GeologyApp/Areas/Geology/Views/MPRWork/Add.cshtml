                                                    </div>
                                                    @if (Model.BoreholeLogs != null && Model.BoreholeLogs != "")
                                                    {
                                                        <label class="form-control-plaintext">@(Model.LogName) <a href='~/@Model.BoreholeLogs' title="" class="ml-2" data-original-title="Download Map Attachment" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                                    }
                                                </div>
                                                <div class="col-lg-3 col-md-6 col-sm-12 form-group">
                                                    <label class="col-form-label font-weight-bolder pt-0">Geological Description</label>
                                                    <div class="custom-file">
                                                        <input type="file" class="custom-file-input" id="SynopsisAttachment" name="SynopsisAttachment">
                                                        <label class="custom-file-label" for="customFile">Choose Files</label>
                                                    </div>
                                                    @if (Model.Synopsis != null && Model.Synopsis != "")
                                                    {
                                                        <label class="form-control-plaintext">@(Model.SynopsisName) <a href='~/@Model.Synopsis' title="" class="ml-2" data-original-title="Download Synopsis Attachment" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                                    }
                                                </div>
                                                <div class="col-lg-3 col-md-6 col-sm-12 form-group">
                                                    <label class="col-form-label font-weight-bolder pt-0">Any Other Information</label>
                                                    <div class="custom-file">
                                                        <input type="file" class="custom-file-input" id="Other_InfoAttachment" name="Other_InfoAttachment">
                                                        <label class="custom-file-label" for="customFile">Choose Files</label>
                                                    </div>
