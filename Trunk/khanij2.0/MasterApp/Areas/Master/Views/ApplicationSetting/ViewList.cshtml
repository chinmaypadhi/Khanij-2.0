@model MasterEF.ApplicationSettingmaster;
@{
    ViewData["Title"] = "ViewList";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}
@if (ViewBag.msg != null)
{
    <script>
        swal("",'@ViewBag.msg',"success");
    </script>
}
<script>

    $(document).ready(function () {
        loadNavigation('ViewList', 'glTMng', 'plApplicationSetting', 'tl', 'Masters', 'Application Setting', '');

        $('.searchableselect').searchableselect();

    });
</script>
<section class="wrapper main-wrapper row">
    <div class="col-12">
        @Html.Partial("_CMSnavigation")
        <!-- MAIN CONTENT AREA STARTS -->
        <div class="row">
            <div class="col-12">
                <div class="main-tab">
                    <ul class="nav nav-tabs">
                        <li class="nav-item">
                            <a class="nav-link" href="~/Master/ApplicationSetting/Add">
                                Add
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" href="javascript:void(0);">
                                View
                            </a>
                        </li>
                    </ul>
                    @Html.Partial("_CMSutil")
                </div>
                <section class="box form-container">
                    @using (Html.BeginForm("Viewlist", "ApplicationSetting", FormMethod.Post, new { AUTOCOMPLETE = "Off", id = "form1" }))
                    {
                        <div class="search-box" style="display:none;">
                            <div class="searchform px-3 pt-3">
                                <div class="row">
                                    <div class="col-sm-5">
                                        <div class="form-group row">
                                            <label for="inputText" class="col-sm-3 col-form-label">
                                                Notice Text
                                            </label>
                                            <div class="col-sm-8">
                                                @Html.TextBoxFor(m => m.EMAIL_SENT, new { @class = "form-control" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-5">
                                        <div class="form-group row">
                                            <div class="col-sm-9 offset-md-3">
                                                <input type="submit" value="Show" name="Show1" />
                                                @*<a href="#" class="btn btn-sm btn-success ml-0"> Show </a>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                    <div class="content-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <table id="table-exports" class="table table-sm table-bordered">
                                    <thead>
                                        <tr>
                                            <th>
                                                Expiry Alert Day
                                            </th>
                                            <th>
                                                Tp Offline Net Weight
                                            </th>
                                            <th>
                                                RTP Trip Close Tolerance Weight
                                            </th>
                                            <th>
                                                Timeline of Reply of Notice
                                            </th>
                                            <th>
                                                Validity Expired Intimation
                                            </th>
                                            <th>
                                                SMS Send
                                            </th>
                                            <th>
                                                Email Send
                                            </th>
                                            <th width="100" class="noprint text-center">Action</th>
                                        </tr>
                                    </thead>

                                    <tbody>
                                        @foreach (var b in ViewBag.ViewList)
                                        {
                                            <tr>
                                                <td>@b.EXPIRY_DATE_ALERT_COUNT </td>
                                                <td>@b.TP_OFFLINE_NET_WEIGHT </td>
                                                <td>@b.FORWARDINGNOTE_GRACE_WEIGHT </td>
                                                <td>@b.TIMELINE_OF_REPLY_OF_NOTICE </td>
                                                <td>@b.EXPIRY_DATE_INTIMATION </td>
                                                <td>@b.SMS_SENT </td>
                                                <td>@b.EMAIL_SENT </td>
                                                <td class="noprint text-center">
                                                    <a href="@Url.Content("~/Master/ApplicationSetting/Add")"
                                                       class="btn-floating btn-outline-primary btn-sm"
                                                       title="Edit"><i class="icon-pencil-alt-solid"></i></a>
                                                    @*<a href="@Url.Content("~/Master/ApplicationSetting/Delete/")@b.DNId"
                                                        class="btn-floating btn-outline-danger btn-sm" onclick="return confirm('Are you sure to Delete?')"
                                                        title="Delete"><i class="far fa-trash-alt"></i></a>*@

                                                </td>
                                            </tr>
                                        }
                                </table>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>

