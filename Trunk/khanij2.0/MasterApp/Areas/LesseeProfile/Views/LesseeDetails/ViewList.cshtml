@*// *********************************************************************************************************
    //  View Name                : Add
    //  Desciption               : To View Lesee Application Details
    //  Created By               : Lingaraj Dalai
    //  Created On               : 06 July 2021
    // ********************************************************************************************************
*@
@model MasterEF.LesseeInfoModel;
@using MasterApp.Web;
@using MasterEF;
@using  Microsoft.AspNetCore.Http;
@inject IHttpContextAccessor HttpContextAccessor
@{
    ViewData["Title"] = "ViewList";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
    UserLoginSession profile = HttpContextAccessor.HttpContext.Session.Get<UserLoginSession>(KeyHelper.UserKey);
    //profile.MineralTypeName = "Major Mineral";
    //profile.MineralName = "COAL";
}
<script>
    backMe = "yes"
    $(document).ready(function () {
        loadNavigation('CompareApplicationDetails', 'glUIS', 'pllesseprof', 'tl', 'Lessee Profile', 'Lesse Details', ' ');
        $('.divPerformAmt').hide();
          /*Show hide controls for Minor/Major-Coal,NonCoal*/
        var mineraltypename = '@profile.MineralTypeName';
        var mineralname = '@profile.MineralName';
        if (mineraltypename == "Minor Mineral") {
            $('.divCoal').hide();/*Field E&I Cess*/
            $('.divBidPremium').hide();/*BidPremium*/
            $('.divLabEstablished').hide();
            $('.divMajor').hide();
        }
        else {
            $('.divMinor').hide();
            if (mineralname != "COAL") {
                $('.divCoal').hide();/*Field E&I Cess*/
                $('.divAuctionDiv').hide();/*Field With Auction Money*/
            }
        }
        if (@profile.UserTypeId!= 7) { /*Other than Lessee Login*/
            $("#navlinkAdd").hide(); //Hide Add tab
            $(".ml-3").hide(); //Hide Edit button
        }
    });
</script>
<section class="wrapper main-wrapper row">
    <div class="col-12">
        <!-- MAIN CONTENT AREA STARTS -->
        @Html.Partial("_CMSnavigation")
        <div class="row">
            <div class="col-12">
                <div class="main-tab">
                    <ul class="nav nav-tabs">
                        <li class="nav-item" id="navlinkAdd">
                            <a class="nav-link" href="~/LesseeProfile/LesseeDetails/Add">
                                Add
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" href="javascript:void(0);">
                                View
                            </a>
                        </li>
                    </ul>
                    @Html.Partial("_CMSutil")
                </div>
                <section class="box form-container">
                    <div class="content-body">
                        <div class="message-heading">
                            <h5 class="font-weight-bold text-brown">Lessee Information</h5>
                            <div>
                                <a href="javascript:void(0);" class="btn-dark btn-sm log-btn" data-val="collapsefillter1">Log Report <i class="icon-angle-down-solid"></i></a>
                                <a href="@CustomQueryStringHelper.EncryptString("LesseeProfile","Add","LesseeDetails",new {id=Model.CREATED_BY })" class="btn-success btn-sm ml-3" data-original-title="Edit"><i class="icon-pencil-alt-solid"></i></a>
                            </div>
                        </div>
                        <div class="message-highlighter">
                            <div class="row">
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Lease Type</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext" id="lblLeaseTypeId"><span class="colon">:</span>@Model.LeaseTypeName</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Application No.</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    @if (Model.APPLICATION_FORM_FILE_NAME != null)
                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span>@(Model.APPLICATION_NUMBER) <a href='~/Upload/Lessee/APPLICATION/APPLICATION_FORM/@Model.APPLICATION_FORM_FILE_NAME' title="" class="ml-2" data-original-title="Download Application Form" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                    }
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Mine Code</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.MINE_CODE</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Lease Name</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.RECONNAISSANCE_LICENSE_NAME</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Lease Address</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    <label class="form-control-plaintext"><span class="colon">:</span>@Model.RECONNAISSANCE_LICENSE_ADDRESS</label>
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">PAN Card No.</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    @if (Model.PAN_CARD_COPY_FILE_NAME != null)
                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span>@(Model.PAN_CARD_NO) <a href='~/Upload/Lessee/APPLICATION/PAN_CARD/@Model.PAN_CARD_COPY_FILE_NAME' title="" class="ml-2" data-original-title="Download PAN Card Copy" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                    }
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Adhar Card No.</label>
                                <div class="col-lg-3 col-md-8 col-sm-12">
                                    @if (Model.ADHAR_CARD_SCAN_COPY_FILE_NAME != null)
                                    {
                                        <label class="form-control-plaintext"><span class="colon">:</span>@Model.ADHAR_CARD <a href='~/Upload/Lessee/APPLICATION/ADHAR_CARD/@Model.ADHAR_CARD_SCAN_COPY_FILE_NAME' title="" class="ml-2" data-original-title="Download AAdhar Card Copy" target="_blank"><i class="icon-file-pdf-solid h5 text-danger"></i></a></label>
                                    }
                                </div>
                                <label class="col-lg-3 col-md-4 col-sm-12 col-form-label">Tin Card No.</label>
