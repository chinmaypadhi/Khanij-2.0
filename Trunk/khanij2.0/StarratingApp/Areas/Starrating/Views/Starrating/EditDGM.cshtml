    /*Bind File Upload Data details*/
    function BindData(Assessmentid, action, id, FMId) {
        var lbid = id;
        debugger;
        $.ajax({
            url: '/Starrating/Starrating/GetUploadData',
            data: { 'AssessmentId': Assessmentid, 'Action': action, 'FMId': FMId },
            dataType: 'json',
            type: 'post',
            success: function (data) {
                debugger;
                for (i = 0; i < data.length; i++) {
                    if (data[i].fileName != "" && data[i].fileExtension != "") {
                        if (FMId > 0) {
                            $(lbid).append(
                                '<a href="../../../Upload/SR_UploadedFiles/' + data[i].fileName + data[i].fileExtension + '" target="_blank"><i class="fas fa-file-pdf" aria-hidden="true"></i></a>');
                        }
                        else if (action == "K") {
                            $(lbid).append(
                                '<label class="col-lg-2 col-md-4 col-sm-12 font-weight-bold">' + data[i].otherName + '</label><div class="col-lg-2 col-md-8 col-sm-12 mb-2"><span class="colon">:</span> <a href="../../../Upload/SR_UploadedFiles/' + data[i].fileName + data[i].fileExtension + '" target="_blank"><i class="icon-download-solid" aria-hidden="true"></i></a></div>');
                        }
                        else {
                            $(lbid).append(
                                '<span class="colon">:</span> <a href="../../../Upload/SR_UploadedFiles/' + data[i].fileName + data[i].fileExtension + '" target="_blank"><i class="icon-download-solid" aria-hidden="true"></i></a>');
                        }
                    }
                }
            },
            errSR_AMSR_AMor: function (err) {
            }
        });

    }
