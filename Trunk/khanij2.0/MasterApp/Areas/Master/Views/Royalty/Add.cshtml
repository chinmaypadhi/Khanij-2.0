                                                            if (item.MineralTypeId == Model.MineralTypeId)
                                                            {
                                                                <option selected="selected" value="@item.MineralTypeId">@item.MineralTypeName</option>
                                                            }
                                                            else
                                                            {
                                                                <option value="@item.MineralTypeId">@item.MineralTypeName</option>
