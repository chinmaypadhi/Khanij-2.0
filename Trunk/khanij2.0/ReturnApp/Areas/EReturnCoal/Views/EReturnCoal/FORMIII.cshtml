    function Get_TotalI() {
        debugger;
        var Opening_stocks = $('#Opening_stocks').val();
        var Coal_raised = $('#Coal_raised').val();

        Opening_stocks = Opening_stocks || 0;
        Coal_raised = Coal_raised || 0;
        var total = parseFloat(Opening_stocks) + parseFloat(Coal_raised);

        $('#TotalI').val(total).trigger("change");
        Calculate_ClosingStock();
    }

    function Calculate_ClosingStock() {

        TotalI = $('#TotalI').val();

        Coaldespatched = $('#Coaldespatched').val();
        Collieryconsumption = $('#Collieryconsumption').val();
        Coalusedforcoking = $('#Coalusedforcoking').val();
        Shortagedueto_Causes = $('#Shortagedueto_Causes').val();
        Coaldespatched = Coaldespatched || 0;
        Collieryconsumption = Collieryconsumption || 0;
        Coalusedforcoking = Coalusedforcoking || 0;
        Shortagedueto_Causes = Shortagedueto_Causes || 0;

        var Total = (parseFloat(TotalI)) - (parseFloat(Coaldespatched) + parseFloat(Collieryconsumption) + parseFloat(Coalusedforcoking) + parseFloat(Shortagedueto_Causes));
        $('#Closingstocks').val(Total).trigger("change");
        Calculate_Total();
    }

    function Calculate_Total() {

        Coaldespatched = $('#Coaldespatched').val();
        Collieryconsumption = $('#Collieryconsumption').val();
        Coalusedforcoking = $('#Coalusedforcoking').val();
        Shortagedueto_Causes = $('#Shortagedueto_Causes').val();
        Closingstocks = $('#Closingstocks').val();

        Coaldespatched = Coaldespatched || 0;
        Collieryconsumption = Collieryconsumption || 0;
        Coalusedforcoking = Coalusedforcoking || 0;
        Shortagedueto_Causes = Shortagedueto_Causes || 0;
        Closingstocks = Closingstocks || 0;

        var Total = parseFloat(Coaldespatched) + parseFloat(Collieryconsumption) + parseFloat(Coalusedforcoking) + parseFloat(Shortagedueto_Causes) + parseFloat(Closingstocks);

        $('#TotalII').val(Total).trigger("change");
    }
    //--------Save Table E a------------
    function SaveTableEa() {
        if (ValidateTableEa() == true) {
            $.ajax({
                url: "/EReturnCoal/EReturnCoal/AddE_EXPLOSIVES",
                data: {
                    'MineralGradeId': $('#ddlMineralGrade').val(), 'Opening_stocks': $('#Opening_stocks').val(), 'Coal_raised': $('#Coal_raised').val(),
                    'Totalvalueofrasing': $('#Totalvalueofrasing').val(), 'TotalI': $('#TotalI').val(), 'Coaldespatched': $('#Coaldespatched').val(),
                    'Collieryconsumption': $('#Collieryconsumption').val(), 'Coalusedforcoking': $('#Coalusedforcoking').val(),
                    'Shortagedueto_Causes': $('#Shortagedueto_Causes').val(), 'Closingstocks': $('#Closingstocks').val(),
                    'TotalII': $('#TotalII').val(), 'Year1': $('#Year2').val()
                },
                dataType: 'json',
                type: 'post',
                success: function (data) {
                    if (data == "1") {
                        alert('Record Created Successfully');
                        GetTablEa();
                        ResetTableEa();
                    }
                    else if (data == "2") {
                        alert('This grade entry already existed.please try again !');
                        ResetTableEa();
                    }
                    else {
                        alert(data);
                    }
                },
                error: function (err) {
                    alert(err.responseText);
                }
            });
        }
    }

    function ResetTableEa() {
        $('#Opening_stocks').val('');
        $('#Coal_raised').val('');
        $('#Totalvalueofrasing').val('');
        $('#TotalI').val('');
        $('#Coaldespatched').val('');
        $('#Collieryconsumption').val('');
        $('#Coalusedforcoking').val('');
        $('#Shortagedueto_Causes').val('');
        $('#Closingstocks').val('');
        $('#TotalII').val('');
        $('#TableEaModal').modal('hide');
    }

    function ValidateTableEa() {
        if ($('#ddlMineralGrade').val() == "") {
            alert('Please Select Mineral Grade');
            $('#ddlMineralGrade').focus();
            return false;
        }

        if ($('#Coal_raised').val() == "") {
            alert('Please Enter Coal Raised');
            $('#Coal_raised').focus();
            return false;
        }

        if ($('#Totalvalueofrasing').val() == "") {
            alert('Please Enter Total value of rasing');
            $('#Totalvalueofrasing').focus();
            return false;
        }

        if ($('#Collieryconsumption').val() == "") {
            alert('Please Enter Consumption (exclusive of coal used for coke making)');
            $('#Collieryconsumption').focus();
            return false;
        }

        if ($('#Coalusedforcoking').val() == "") {
            alert('Please Enter Coal used for coking, if any, on colliery');
            $('#Coalusedforcoking').focus();
            return false;
        }

        if ($('#Shortagedueto_Causes').val() == "") {
            alert('Please Enter Shortage due to fire,rains and other causes');
            $('#Shortagedueto_Causes').focus();
            return false;
        }

        return true;
    }

    //Delete Table A
    function DeleteTableEa(par) {

        $.ajax({
            url: "/EReturnCoal/EReturnCoal/DeleteE_EXPLOSIVES",
            data: { 'Id': par },
            dataType: 'json',
            type: 'post',
            success: function (data) {
                if (data == "1") {
                    alert("Record Deleted Successfully");
                    GetTablEa();
                }
                else {
                    alert("Some error occur.Please Try after some time");
                }
            },
            error: function (XHR) {
                alert(XHR.responseText);
            }

        });
    }
