@*// *********************************************************************************************************
    //  View Name                : VehicleRenewal
    //  Desciption               : Vehicle Renewal Details
    //  Created By               : Akshaya Dalei
    //  Created On               : 25 July 2021
    // ********************************************************************************************************
*@
@{
    ViewBag.Title = "Vehicle Renewal";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}
@model IEnumerable<userregistrationEF.Vehicle>

<section class="wrapper main-wrapper row">
    <div class="col-12">
        @Html.Partial("_CMSnavigation")
        <!-- MAIN CONTENT AREA STARTS -->
        <div class="row">
            <div class="col-12">
                <div class="main-tab">
                    <ul class="nav nav-tabs">
                        <li class="nav-item">
                            <a class="nav-link active" href="javascript:void(0);">
                                Vehicle Renewal Details
                            </a> 
                        </li>
                    </ul>
                    @Html.Partial("_CMSutil")
                </div>

                <section class="box form-container">
                    <div class="content-body">
                        <div class="row">
                            <div class="col-sm-12">
                                <table id="table-exports" class="table table-sm table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Sr No</th>
                                            <th>Vehicle Number</th>
                                            <th>Validity From</th>
                                            <th>Validity To</th>
                                            <th>Payment Referance No.</th>
                                            <th>Bank</th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        @{

                                            foreach (var item in Model)
                                            {

                                                <tr class="active-row">
                                                    <td><p class="m-0">@item.SrNo</p></td>
                                                    <td><p class="m-0">@item.VehicleNumber</p></td>
                                                    <td><p class="m-0">@item.VehicleValidFromDate</p> </td>
                                                    <td><p class="m-0">@item.VehicleValidToDate</p> </td>
                                                    <td><p class="m-0">@item.PaymentReqID</p> </td>
                                                    <td><p class="m-0">@item.BankName</p> </td>
                                                </tr>
                                            }

                                        }
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>
<script>
    $(document).ready(function () {
        loadNavigation('Vehicles Payment', 'ql', 'pl', 'tl', 'Vehicle', 'Vehicle Renewal Details', ' ');
    });
</script>