<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="bin\Debug\net5.0\SupportApp.exe" arguments="http://khanijsupportapp.khanij.com/" stdoutLogEnabled="false" hostingModel="InProcess">
        <environmentVariables>
          <environmentVariable name="SupportKey" value="SupportValue" />
        </environmentVariables>
      </aspNetCore>
    </system.webServer>
  </location>
</configuration>