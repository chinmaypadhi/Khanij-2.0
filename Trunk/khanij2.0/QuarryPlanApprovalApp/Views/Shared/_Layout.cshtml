<!DOCTYPE html>
<html>
<head>
    <title>Khanij Online </title>
    <meta charset="utf-8" />
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link rel="stylesheet" href='@Url.Content("~/css/bootstrap-select.min.css")' />
    <link rel="stylesheet" href='@Url.Content("~/css/bootstrap.min.css")' />
    <link rel="stylesheet" href='@Url.Content("~/css/perfect-scrollbar.css")' />
    <link rel="stylesheet" href='@Url.Content("~/css/iconslist.css")' type="text/css" />
    <link rel="stylesheet" href='@Url.Content("~/css/style.css")' type="text/css" />
    <link rel="stylesheet" href='@Url.Content("~/css/mineral-concession.css")' type="text/css" />
    <script src='@Url.Content("~/js/jquery.min.js")'></script>
    <script src='@Url.Content("~/js/sweetalert.min.js")'></script>
    <link href='@Url.Content("~/css/bootstrap-datetimepicker.min.css")' rel="stylesheet">
    <script src='@Url.Content("~/js/moment.min.js")'></script>
    <script src='@Url.Content("~/js/bootstrap-datetimepicker.min.js")'></script>
</head>
<body>
    @Html.Partial("_navbar")
    @Html.Partial("_navigation")
    <div class="page-container">
        <div class="page-sidebar fixedscroll">
            @Html.Partial("_leftsider")
        </div>
        <div id="main-content">
            @RenderBody()
            @Html.Partial("_footer")
        </div>
    </div>
  
    <link rel="stylesheet" href='@Url.Content("~/css/searchable-select.min.css")'>
    <script src='@Url.Content("~/js/searchable-select.min.js")'></script>
    <link rel="stylesheet" href='@Url.Content("~/css/dataTables.bootstrap4.min.css")'>
    <script src='@Url.Content("~/js/jquery.dataTables.min.js")'></script>
    <script src='@Url.Content("~/js/dataTables.bootstrap4.min.js")'></script>

    @RenderSection("Scripts", required: false)

</body>
</html>
