                                                if (Model != null && Model.GrossWeight_Qty != null && Model.GrossWeight_Qty > 0)
                                                {
                                                    @(Html.DisplayFor(model => model.TareWeight_Qty)) <label> @Model.UnitName</label>
                                                }
                                                else
                                                {
                                                    <br />
                                                    <span></span>
                                                }
                                            }
                                            else
                                            {
                                                @(Html.DisplayFor(model => model.TareWeight_Qty)) <label> @Model.UnitName</label>
                                            }

                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <strong>Gross Weight of the Vehicle (वाहन का ग्रॉस वेट (</strong>
