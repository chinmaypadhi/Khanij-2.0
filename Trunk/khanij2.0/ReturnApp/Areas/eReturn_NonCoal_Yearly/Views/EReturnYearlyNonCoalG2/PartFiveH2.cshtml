    function MakeNumeric(obj) {
        if (event.shiftKey == true) {
            event.preventDefault();
        }

        if ((event.keyCode >= 48 && event.keyCode <= 57) ||
            (event.keyCode >= 96 && event.keyCode <= 105) ||
            event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 37 ||
            event.keyCode == 39 || event.keyCode == 46 || event.keyCode == 190) {

            var val = $(obj).val();
            if (isNaN(val)) {
                val = val.replace(/[^0-9\.]/g, '');
                if (val.split('.').length > 2) {
                    val = 0;
                    $(obj).val(val);
                    event.preventDefault();
                    return false;
                }
            }
        }
        else {
            event.preventDefault();
        }

        return true;
    }

    //---------------Save Type Of Machinery---------------
    function ValidateTypeOfMachinery() {
        if ($('#TypeofMachinery').val() == "") {
            $('#TypeofMachinery').focus();
            alert('Please Select Type of Machinery');
            return false;
        }
        if ($('#Capacity').val() == "") {
            $('#Capacity').focus();
            alert('Please Enter Capacity');
            return false;
        }
        if ($('#Capacity_Unit').val() == "") {
            $('#Capacity_Unit').focus();
            alert('Please Enter Capacity Unit');
            return false;
        }
        if ($('#NoofMachinery').val() == "") {
            $('#NoofMachinery').focus();
            alert('Please Enter No Of Machinery');
            return false;
        }
        return true;
    }

    function SaveTypeOfMachinery() {
        debugger;
        if (ValidateTypeOfMachinery() == true) {
            $.ajax({
                url: "/eReturn_NonCoal_Yearly/EReturnYearlyNonCoalG2/AddMachineryDetails",
                data: {
                    'TypeofMachinery': $('#TypeofMachinery').val(), 'Capacity': $('#Capacity').val(),
                    'Capacity_Unit': $('#Capacity_Unit').val(), 'NoofMachinery': $('#NoofMachinery').val(),
                    'Type_Electrical': $('input[name="Type_Electrical"]:checked').val(), 'Used_Area': $('input[name="Used_Area"]:checked').val(), 'Year': $('#Year').val()
                },
                dataType: 'json',
                type: 'post',
                success: function (data) {
                    if (data == "1") {
                        alert('Record Created Successfully');
                        GetMachineryDetails();
                        Clear();
                    }
                    else if (data == "2") {
                        alert('This Record Already Exist');
                        GetMachineryDetails();
                        Clear();
                    }
                    else {
                        alert('Transaction Failed.Please Try after some time!');
                    }
                },
                error: function (err) {
                    alert(err.responseText);
                }
            });
        }
    }

